// BEGIN: DB.GapConductanceModel_1

// Radial gap conductance model that uses a constant heff for the fuel outer surface 
// and one on the cladding that is modified to account for the change is surface area.
//   h_{fuel}  = 2920 W/m^2-K
//   THICKNESS = myRadius - otherRadius
//   RADIUS    = myRadius
P2CThermalModel{
  name      = "GapConductanceModelRadial"
  CONSTANT  =  2920.0
}
C2PThermalModel{
  name      = "GapConductanceModelRadial"
  CONSTANT  =  2920.0
  THICKNESS  = -8.5e-05
  RADIUS     = 0.00418
}

// END:   DB.GapConductanceModel_1


