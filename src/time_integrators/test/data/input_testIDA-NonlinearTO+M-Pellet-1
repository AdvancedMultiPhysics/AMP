VolumeIntegralOperator{
  name="VolumeIntegralOperator"
  InputVariableType = "IntegrationPointScalar"
  Number_Active_Variables = 1
  Number_Auxillary_Variables = 0
  ConstantSource   = FALSE
  ActiveInputVariables{
    ActiveVariable_0 = "SpecificPowerInWattsPerGram" 
  }
  OutputVariable = "Temperature"
  print_info_level = 3

  SourcePhysicsModel {
    name = "SourcePhysicsModel"
    Material = "UO2_MSRZC_09"
    USE_ELEMENT_PHYSICS = "MassDensityModel"
    Equation = "ThermalSource"
    USE_MATERIALS_LIBRARY = TRUE 
     Default_Concentration = .1
     Default_Temperature   = 400.0
     Default_Burnup   = 0.0
  }

  SourceElement {
    name="SourceNonlinearElement"
    print_info_level = 3
    FE_ORDER = "FIRST"  
    FE_FAMILY = "LAGRANGE"
    QRULE_TYPE = "QGAUSS"
    QRULE_ORDER = "DEFAULT"
  }
}


ICLinearSolver {
  print_info_level = 10
    max_iterations = 1000
    max_error = 1e-10
    ksp_type = "fgmres"
    absolute_tolerance = 1.0e-12
    relative_tolerance = 1.0e-10
    divergence_tolerance = 1.0e03
    max_krylov_dimension = 40
    uses_preconditioner=TRUE
    pc_type = "shell"
    pc_side = "RIGHT"

    KSPOptions = "-ksp_monitor -ksp_converged_reason"

    Preconditioner{
      print_info_level = 3
        max_iterations = 1
        max_error = 1e-10
        pelletPreconditioner{
          print_info_level = 6
            max_iterations = 1
            max_error = 1e-10
        }

    }

}


IDATimeIntegrator{
                name = "IDATimeIntegrator"
  print_info_level = 0
    linear_solver_type		= 1
    relative_tolerance		= 1.00e-10
    absolute_tolerance		= 1.00e-8
    initial_time 		= 0.0
    final_time			= 100000.0
    max_integrator_steps	= 10000
    initial_dt                  = 1.0e-07
    max_dt			= 10.0
    min_dt			= 1.0e-09
    bLinearMassOperator         = TRUE
    bLinearRhsOperator          = FALSE
    CallCalcIC=FALSE
    usePreconditioner           = TRUE
    algebraicComponent = 2

    Preconditioner{
      print_info_level = 6
        max_iterations = 1
        max_error = 1e-10
        pelletThermalPreconditioner{
          print_info_level = 6
            max_iterations = 1
            max_error = 1e-10
        }

      pelletOxygenPreconditioner{
        print_info_level = 6
          max_iterations = 1
          max_error = 1e-10
      }

      pelletMechanicsPreconditioner{
        print_info_level = 6
          PDE_equations = 1
          max_levels = 10
          max_iterations = 1
          max_error = 1.0e-10
          //      increasingordecreasing = "increasing"
          //      aggregationtype = "Uncoupled"
          //      smoothertype = "Chebyshev"
          smoother_sweeps = 3
          smoother_preorpost = "both"
          coarse_type="Amesos-KLU"
      }

    }
}

MechanicsNonlinearSolver{
  print_info_level = 7
    max_iterations = 500
    max_error = 1e-10
    absolute_tolerance = 1.0e-16
    relative_tolerance = 1.0e-16
    stepTolerance = 1.0e-16
    maximumFunctionEvals = 10000
    usesJacobian=FALSE

    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ksp_ew -snes_ls_maxstep 10000.0 -snes_ls_alpha 1.0e-4"

    LinearSolver{
      print_info_level = 0
        max_iterations = 100
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-9
        relative_tolerance = 1.0e-09
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason -ksp_max_it 100 -ksp_rtol 1.0e-9 -ksp_atol 1.0e-9"

        Preconditioner{
          print_info_level = 10
            PDE_equations = 3
            max_levels = 10
            max_iterations = 1
            max_error = 1.0e-10
            increasingordecreasing = "increasing"
            aggregationtype = "Uncoupled"
            smoothertype = "Chebyshev"
            smoother_sweeps = 3
            smoother_preorpost = "both"
            coarse_type="Amesos-KLU"
        }
    }
}


