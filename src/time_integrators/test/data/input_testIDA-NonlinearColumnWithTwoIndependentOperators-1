NumberOfMeshes = 1
Mesh_1{
  Filename = "unitCubeDirichlet.e"
  MeshName = "ida"
  x_offset = 0.0
  y_offset = 0.0
  z_offset = 0.0
  NumberOfElements = 1000
}
NeutronicsOperator{
  name="NeutronicsRhsOperator"
  type               = "Power"
  OutputVariable     = "SpecificPowerInWattsPerGram"
  numTimeSteps       = 2
  timeSteps          = 1, 10000.
  useFixedValue      = TRUE
  fixedValues        = 1000.0, 2.0
  print_info_level   = 3
}

VolumeIntegralOperator{
  name="VolumeIntegralOperator"
  InputVariableType = "IntegrationPointScalar"
  Number_Active_Variables = 1
  Number_Auxillary_Variables = 0
  ConstantSource   = FALSE
  ActiveInputVariables{
    ActiveVariable_0 = "SpecificPowerInWattsPerGram" 
  }
  OutputVariable = "HeatSource"
  print_info_level = 3

  SourcePhysicsModel {
    name = "SourcePhysicsModel"
    Equation="ThermalSource"
//    Material = "UO2_MSRZC_09"
    Material = "Independent"
    USE_ELEMENT_PHYSICS = "MassDensityModel"
    USE_MATERIALS_LIBRARY = TRUE 
    BilogScaleCoefficient = FALSE
  }

  SourceElement {
    name="SourceNonlinearElement"
    print_info_level = 0
    FE_ORDER = "FIRST"  
    FE_FAMILY = "LAGRANGE"
    QRULE_TYPE = "QGAUSS"
    QRULE_ORDER = "DEFAULT"
  }
}


NonlinearThermalOperator1 {
   name="NonlinearBVPOperator"
   print_info_level = 3
  
   VolumeOperator {
     name = "DiffusionNonlinearFEOperator" 
     print_info_level = 1
     ActiveInputVariables {
         temperature = "temperature1"
     }
     OutputVariable = "temperature1"
     PrincipalVariable = "temperature" 

     DiffusionElement{
        name = "DiffusionNonlinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
     }    

     DiffusionTransportModel{
        name = "DiffusionTransportModel"
        Material = "UO2_MSRZC_09"
        Property = "ThermalConductivity"
        Default_Concentration = .1
        Default_Temperature   = 750.
        Default_Burnup   = 0.0
//        UseBilogScaling = TRUE
//        BilogVariable = "concentration"
        BilogScaleCoefficient = FALSE
     } 
   }

   BoundaryOperator {
      name = "DirichletVectorCorrection"
      isAttachedToVolumeOperator=TRUE
      print_info_level = 1
      skip_params = FALSE
      number_of_ids = 1
      id_0 = 10
      number_of_dofs_0 = 1
      dof_0_0 = 0
      value_0_0 = 750.
   }
}

LinearThermalOperator1 {
   name="LinearBVPOperator"
   print_info_level = 1

   VolumeOperator{
    name = "DiffusionLinearFEOperator"
    InputVariable = "temperature1"
    OutputVariable = "temperature1"
    print_info_level = 1
    reset_reuses_matrix = TRUE
    FixedConcentration = TRUE
    FixedBurnup = TRUE

    DiffusionElement {
        name = "DiffusionLinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

     DiffusionTransportModel{
        name = "DiffusionTransportModel"
        Material = "UO2_MSRZC_09"
        Property = "ThermalConductivity"
        Default_Concentration = .1
        Default_Temperature   = 750.
        Default_Burnup   = 0.0
//        UseBilogScaling = TRUE
//        BilogVariable = "concentration"
       BilogScaleCoefficient = FALSE
     } 
  }


BoundaryOperator {
    name="DirichletMatrixCorrection"
    skip_params = FALSE
    skip_rhs_correction = TRUE
    skip_rhs_add_correction = TRUE
    print_info_level = 3
    number_of_ids = 1
    id_0             = 10
    number_of_dofs_0 =   1
    dof_0_0          =   0
    value_0_0        = 0.
  }
}

