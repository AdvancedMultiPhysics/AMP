IDATimeIntegrator{
                name = "IDATimeIntegrator"
		linear_solver_type		= 1
		relative_tolerance		= 1.00e-10
		absolute_tolerance		= 1.00e-16
		initial_time 			= 0.0
                             final_time			= 1000.0
		max_integrator_steps		= 10000
                             initial_dt                                            = 0.01
		max_dt				= 10.0
		min_dt				= 1.0e-08
		bLinearMassOperator             = TRUE
                             bLinearRhsOperator              = FALSE
                             CallCalcIC=TRUE
	               usePreconditioner                 = TRUE

                             Preconditioner{
                                    print_info_level = 3
                                    max_iterations = 1
                                    max_error = 1e-10
                                    thermalPreconditioner1{
                                             print_info_level = 3
                                             max_iterations = 1
                                             max_error = 1e-10
                                   }
                                    thermalPreconditioner2{
                                             print_info_level = 3
                                             max_iterations = 1
                                             max_error = 1e-10
                                     }
                             }
}




