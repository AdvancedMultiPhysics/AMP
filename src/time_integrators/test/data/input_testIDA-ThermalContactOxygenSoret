  }
}

NonlinearThermalOperator1 {
  name="NonlinearBVPOperator"
    print_info_level = 1

    VolumeOperator {
      name = "DiffusionNonlinearFEOperator" 
        print_info_level = 6
        ActiveInputVariables {
         temperature = "Temperature"
         concentration = "Concentration"
        }
      OutputVariable = "Temperature"
        PrincipalVariable = "temperature" 

        DiffusionElement{
          name = "DiffusionNonlinearElement"
            TransportAtGaussPoints = TRUE
            FE_ORDER = "FIRST"  
            FE_FAMILY = "LAGRANGE"
            QRULE_TYPE = "QGAUSS"
            QRULE_ORDER = "DEFAULT"
        }    

      DiffusionTransportModel{
        name = "DiffusionTransportModel"
        Material = "UO2_MSRZC_09"
          Property = "ThermalConductivity"
          Default_Concentration = .1
          Default_Temperature   = 557.2
          Default_Burnup   = 0.0
      } 
    }

  BoundaryOperator {
      name = "RobinVectorCorrection"
      FE_ORDER = "FIRST"  
      FE_FAMILY = "LAGRANGE"
      QRULE_TYPE = "QGAUSS"
      QRULE_ORDER = "DEFAULT"
      alpha          = 1.0
      beta           = 2920.0
      gamma          = 2920.0
      print_info_level = 3
      skip_params = FALSE
      number_of_ids = 1
      IsCoupledBoundary_0 = TRUE
      id_0         =   4
      number_of_dofs_0 =   1
      dof_0_0          =   0
      value_0_0        =  0.0
  }
}

