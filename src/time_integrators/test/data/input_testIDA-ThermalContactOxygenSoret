          Default_Burnup   = 0.0
      } 
    }

 BoundaryOperator {
   name="ColumnBoundaryOperator"
   numberOfBoundaryOperators=2
   boundaryOperators="RobinVectorCorrection","DirichletVectorCorrection"

   RobinVectorCorrection {
   name="RobinVectorCorrection"
   FE_ORDER = "FIRST"  
   FE_FAMILY = "LAGRANGE"
   QRULE_TYPE = "QGAUSS"
   QRULE_ORDER = "DEFAULT"
   alpha          = 1.0
   beta           = 2920.0
   gamma          = 2920.0
   print_info_level = 3
   skip_params = FALSE
   number_of_ids = 1
   IsCoupledBoundary_0 = TRUE
   id_0         =   8
   number_of_dofs_0 =   1
   dof_0_0          =   0
   value_0_0        =  0.
   }

   DirichletVectorCorrection{
   name="DirichletVectorCorrection"
   skip_params = FALSE
   isAttachedToVolumeOperator = TRUE
   skip_rhs_correction = FALSE
   skip_rhs_add_correction = FALSE
   print_info_level = 3
   number_of_ids = 1
   id_0             =   4
   number_of_dofs_0 =   1
