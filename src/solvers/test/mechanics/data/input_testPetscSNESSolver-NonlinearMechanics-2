NonlinearSolver{
  print_info_level = 7
    max_iterations = 100
    max_error = 1e-10

    absolute_tolerance = 1.0e-12
    relative_tolerance = 1.0e-12
    stepTolerance = 1.0e-12
    maximumFunctionEvals = 1000
    usesJacobian=TRUE

    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ls_alpha 0.01 -snes_stol=0"

    LinearSolver{
      print_info_level = 1
        max_iterations = 12
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-9
        relative_tolerance = 1.0e-2
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason"

        Preconditioner{
          problem_type = "SA"
            print_info_level = 1
            PDE_equations = 3
            max_iterations = 1
            max_error = 1.0e-10
              aggregationtype = "Uncoupled"
              smoothertype = "Chebyshev"
              smoother_sweeps = 3
        }

    }

}

