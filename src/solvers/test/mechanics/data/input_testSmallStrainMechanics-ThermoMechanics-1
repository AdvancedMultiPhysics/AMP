Load_Boundary {
  name = "DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    setResidual = FALSE
    valuesType = 1
    isAttachedToVolumeOperator = FALSE

    number_of_ids = 1
    id_0 = 3
    number_of_dofs_0 = 1
    dof_0_0 = 2
    value_0_0 = 0.0
    //value_0_0 = 1.0e+8
}

NonlinearSolver{
  print_info_level = 2 
    max_iterations = 100
    absolute_tolerance = 1.0e-15
    relative_tolerance = 1.0e-12
    stepTolerance = 1.0e-18
    maximumFunctionEvals = 1000
    usesJacobian=FALSE
    forcing_term_strategy            =  "CONSTANT"
    constant_forcing_term            =  0.01
//    forcing_term_strategy            =  "EWCHOICE3"
//    EW_choice2_alpha = 2.0
//    EW_choice2_gamma = 0.9

    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ksp_ew -snes_ls_alpha 0.0001"

    LinearSolver{
        name = "PetscKrylovSolver"
        print_info_level = 1
        max_iterations = 100
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-15
        relative_tolerance = 1.0e-15
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 100
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason"

        Preconditioner{
            name = "TrilinosMLSolver"
            print_info_level = 1
            PDE_equations = 3
            max_iterations = 1
            max_levels = 1
            aggregationtype = "Uncoupled"
            smoothertype = "Chebyshev"
            smoother_sweeps = 3
        }
    }
}

