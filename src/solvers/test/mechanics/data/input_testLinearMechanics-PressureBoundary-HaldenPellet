
Mesh{
    FileName = "halden_1_pellet_9mil.e"
    MeshType = "libMesh"
    MeshName = "Pellet"
    dim      = 3
    x_offset = 0
    y_offset = 0
    z_offset = 0
    NumberOfElements = 5226
}

MechanicsBVPOperator {
  name="LinearBVPOperator"
    print_info_level = 1

    VolumeOperator="MechanicsLinearFEOperator"
    BoundaryOperator="DirichletMatrixCorrection"
}

Load_Boundary {
  name="DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    isAttachedToVolumeOperator = FALSE
    setResidual = FALSE
    valuesType = 1
    number_of_ids = 1
    id_0 = 3
    number_of_dofs_0 = 1
    dof_0_0 = 2
    value_0_0 = 0.0
    //value_0_0 = 1.0e6
}

Pressure_Boundary {
  name = "PressureBoundaryOperator"
  BoundaryID = 4
  Value = -1.0e6
  ResidualMode = FALSE
  Variable = "displacement"
}

LinearSolver {
  print_info_level = 10
    max_iterations = 1000
    max_error = 1e-10
    ksp_type = "fgmres"
    absolute_tolerance = 1.0e-16
    relative_tolerance = 1.0e-13
    divergence_tolerance = 1.0e03
    max_krylov_dimension = 40
    uses_preconditioner=TRUE
    pc_type = "shell"
    pc_side = "RIGHT"

    KSPOptions = "-ksp_monitor -ksp_converged_reason -ksp_max_it 1000"

    Preconditioner {
          problem_type = "SA"
          print_info_level = 1
          PDE_equations = 3
          max_iterations = 1
          max_error = 1.0e-16
    }
}

MechanicsLinearFEOperator{
      name="MechanicsLinearFEOperator"
        print_info_level = 1
        reset_reuses_matrix = TRUE
        InputVariable = "displacement"
        OutputVariable = "displacement"

        MechanicsElement {
          name="MechanicsLinearElement"
            USE_REDUCED_INTEGRATION = FALSE 
            FE_ORDER = "FIRST"  
            FE_FAMILY = "LAGRANGE"
            QRULE_TYPE = "QGAUSS"
            QRULE_ORDER = "DEFAULT"
        }
	LocalModel = "MechanicsMaterialModel"
    }

DirichletMatrixCorrection{
    name="DirichletMatrixCorrection"
      print_info_level = 1
      skip_params = FALSE
      symmetric_correction = TRUE
      zero_dirichlet_block = FALSE
      skip_rhs_correction = TRUE

      number_of_ids = 3
     
      id_0 = 101
