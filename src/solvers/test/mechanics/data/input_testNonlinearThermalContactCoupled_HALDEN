VolumeIntegralOperator{
  name="VolumeIntegralOperator"
    InputVariableType = "IntegrationPointScalar"
    Number_Active_Variables = 1
    Number_Auxillary_Variables = 0
    ConstantSource   = FALSE
    ActiveInputVariables{
      ActiveVariable_0 = "SpecificPowerInWattsPerGram" 
    }
  OutputVariable = "heatsource"
    print_info_level = 3
    LocalModel = "SourcePhysicsModel"

    SourceElement {
      name="SourceNonlinearElement"
        print_info_level = 3
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }
}

NonlinearSolver{
  print_info_level = 7
    max_iterations = 500
    max_error = 1e-10

    absolute_tolerance = 1.0e-10
    relative_tolerance = 1.0e-10
    stepTolerance = 1.0e-10
    maximumFunctionEvals = 10000
    usesJacobian=FALSE

    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ls_maxstep 10000.0 -snes_ls_alpha 1.0e-2"

    LinearSolver{
      print_info_level = 0
        max_iterations = 10000
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-9
        relative_tolerance = 1.0e-01
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason -ksp_max_it 10000 -ksp_rtol 1.0e-1 -ksp_atol 1.0e-9"

        Preconditioner{
          print_info_level = 0
            max_iterations = 1
            max_error = 1.0e-10
            absolute_tolerance = 1.0e-10
            relative_tolerance = 1.0e-10
            divergence_tolerance = 1.0e03
            pelletPreconditioner{
              print_info_level = 0
                max_iterations = 1
                max_error = 1.0e-15
            }
          cladPreconditioner{
            print_info_level = 0
              max_iterations = 1
              max_error = 1.0e-15
          }

        }
    }
}


DiffusionNonlinearFEOperator1{
  name = "DiffusionNonlinearFEOperator" 
    print_info_level = 6
    ActiveInputVariables {
      temperature = "Temperature"
    }
  OutputVariable = "Temperature"
    PrincipalVariable = "temperature" 

    DiffusionElement{
      name = "DiffusionNonlinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    
  LocalModel="DiffusionTransportModel1"
}

