
max_iterations = 60

InitialGuess = 400.00

// Database for all the meshes
Mesh{
    // This is the main multimesh that will contain all meshes
    MeshName = "MainMesh"
    MeshType = "Multimesh"
    MeshDatabasePrefix = "Mesh_"
    MeshArrayDatabasePrefix = "MeshArray_"
    Mesh_1 {
        MeshName = "pellet"
        MeshType = "AMP"
        Generator = "cylinder"
        Size = 13, 8
        Range = 0.00533908, 0, 0.0127
        surfaceIds = 4, 4, 4, 4, 1, 2
        static = TRUE
    }
    Mesh_2 {
        MeshName = "clad"
        MeshType = "AMP"
        Generator = "tube"
        Size = 2, 132, 17
        Range = 5.46e-3, 6.35e-3, 0, 0.0127
        surfaceIds = 8, 4, -1, -1, 1, 2
        static = TRUE
    } 
}

NeutronicsOperator{
  name="NeutronicsRhsOperator"
    type               = "Power"
    OutputVariable     = "SpecificPowerInWattsPerGram"
    numTimeSteps       = 1
    timeSteps          = .1
    useFixedValue      = TRUE
    fixedValues        = 4045.3147905 
    print_info_level   = 3
}


MapCladtoPellet{
  BoundaryId = 0
    name="MapSurface"

    Map3Dto1D{
      BoundaryId = 8
        InputVariable = "Temperature"
        OutputVariable = "Gap"
    }

  Map1Dto3D{
    BoundaryId = 4
      InputVariable = "Gap"
      OutputVariable = "TemperatureMap"
  }
}

