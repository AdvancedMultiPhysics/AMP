
Mesh{
    MeshType = "libMesh"
    dim = 3
    MeshName = "TestMesh"
    FileName = "brick.e"
}

NumberOfLoadingSteps = 50

ExtractStressStrainData = FALSE

NonlinearMechanicsOperator {
  name="NonlinearBVPOperator"
    print_info_level = 1
    VolumeOperator = "MechanicsNonlinearFEOperator1"
    BoundaryOperator = "DirichletVectorCorrection1"
}

MechanicsNonlinearFEOperator1 {
      name = "MechanicsNonlinearFEOperator"
        print_info_level = 1
        ActiveInputVariables {
          DISPLACEMENT = "displacement"
        }
      OutputVariable = "displacement"
        RESET_REUSES_RADIAL_RETURN = TRUE
        JACOBIAN_REUSES_RADIAL_RETURN = TRUE

        MechanicsElement {
          name="MechanicsNonlinearElement"
            USE_REDUCED_INTEGRATION = FALSE 
            FE_ORDER = "FIRST"  
            FE_FAMILY = "LAGRANGE"
            QRULE_TYPE = "QGAUSS"
            QRULE_ORDER = "DEFAULT"
        }

	LocalModel = "PericElastoViscoPlasticModel"
}
	
DirichletVectorCorrection1 {
    name = "DirichletVectorCorrection"
      print_info_level = 1
      skip_params = FALSE
      setResidual = FALSE
      valuesType = 1
      isAttachedToVolumeOperator = TRUE
      
      number_of_ids = 2

      id_0 = 2
      number_of_dofs_0 = 3
      dof_0_0 = 0
      dof_0_1 = 1
      dof_0_2 = 2
      value_0_0 = 0.0
      value_0_1 = 0.0
      value_0_2 = 0.0

      id_1 = 1
      number_of_dofs_1 = 1
      dof_1_0 = 2
      value_1_0 = 0.001
  }

PericElastoViscoPlasticModel {
        USE_MATERIALS_LIBRARY = FALSE
          Material = "UO2_MSRZC_09"
          print_info_level = 6
          name="PericElastoViscoPlasticModel"
          //name="IsotropicElasticModel"
