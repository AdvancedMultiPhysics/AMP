          Poissons_Ratio = 0.3
          Linear_Strain_Hardening = 15.0
          Elastic_Yield_Stress = 0.15
          Viscosity = 20.0
          Strain_Exponent = 0.5
          THERMAL_EXPANSION_COEFFICIENT = 2.0e-6
          Default_Temperature = 310.0
          Default_Burnup = 0.0
          Default_Oxygen_Concentration = 0.0
          //name="VonMises_IsotropicKinematicHardening"
