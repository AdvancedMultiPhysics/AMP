// The following parameters are used for the Cook's Membrane Plasticity test
          //Isotropic_Linear_Hardening = 0.015
          //Kinematic_Linear_Hardening = 0.135
          //Initial_Yield_Strength = 0.243
// Parameters for the benchmark test ends here
          //Default_Temperature = 310.0
          //Default_Burnup = 0.0
          //Default_Oxygen_Concentration = .1
      }

LinearMechanicsOperator {
    name="LinearBVPOperator"
    print_info_level = 1
    VolumeOperator = "MechanicsLinearFEOperator2"
    BoundaryOperator = "DirichletMatrixCorrection2"
}

MechanicsLinearFEOperator2 {
      name = "MechanicsLinearFEOperator"
        print_info_level = 1
        reset_reuses_matrix = TRUE
        InputVariable = "displacement"
        OutputVariable = "displacement"

        MechanicsElement {
          name="MechanicsLinearElement"
            USE_REDUCED_INTEGRATION = FALSE 
            FE_ORDER = "FIRST"  
            FE_FAMILY = "LAGRANGE"
            QRULE_TYPE = "QGAUSS"
            QRULE_ORDER = "DEFAULT"
        }
    }

DirichletMatrixCorrection2 {
    name = "DirichletMatrixCorrection"
      print_info_level = 1
      skip_params = FALSE
      symmetric_correction = TRUE
      zero_dirichlet_block = FALSE
      skip_rhs_correction = TRUE
      
      number_of_ids = 2

      id_0 = 2
      number_of_dofs_0 = 3
      dof_0_0 = 0
      dof_0_1 = 1
      dof_0_2 = 2

      id_1 = 1
      number_of_dofs_1 = 1
      dof_1_0 = 2
  }

Load_Boundary {
  name = "DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    setResidual = FALSE
    valuesType = 1
    isAttachedToVolumeOperator = FALSE
    
    number_of_ids = 1
    
    id_0 = 1
    number_of_dofs_0 = 1
    dof_0_0 = 2
    value_0_0 = 0.0
}

NonlinearSolver{
  print_info_level = 2 
    max_iterations = 100
    max_error = 1e-10

    absolute_tolerance = 1.0e-15
    relative_tolerance = 1.0e-12
    stepTolerance = 1.0e-10
    maximumFunctionEvals = 1000
    usesJacobian=TRUE

    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ksp_ew -snes_ls_alpha 0.001"

    LinearSolver{
      print_info_level = 1
        max_iterations = 10
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-15
        relative_tolerance = 1.0e-13
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason"

        Preconditioner{
          print_info_level = 1
            PDE_equations = 3
            max_iterations = 1
            max_error = 1.0e-15
        }
    }
}

