ThermalInitialGuess {
  name = "DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    number_of_ids = 2
    id_0 = 2
    number_of_dofs_0 = 1
    dof_0_0 = 0
    value_0_0 = 1000.0
    id_1 = 3
    number_of_dofs_1 = 1
    dof_1_0 = 0
    value_1_0 = 350.0
}

NonlinearSolver{
    print_info_level = 2
    max_iterations = 100
    absolute_tolerance = 1.0e-12
    relative_tolerance = 1.0e-14
    stepTolerance = 1.0e-16
    maximumFunctionEvals = 1000
    usesJacobian=FALSE

    forcing_term_strategy =  "EWCHOICE3"
    EW_choice2_alpha      = 2.0
    EW_choice2_gamma      = 0.9

#    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ls_alpha 0.01"

    LinearSolver{
        name = "PetscKrylovSolver"
        print_info_level = 10
        max_iterations = 100
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-9
        relative_tolerance = 0.1
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 100
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason -ksp_max_it 100"

        Preconditioner{
            name = "ColumnSolver"
            print_info_level = 1
            max_iterations = 1
            absolute_tolerance = 1.0e-16
            relative_tolerance = 1.0e-01
            solvers = "thermalPreconditioner", "mechanicsPreconditioner"

            thermalPreconditioner{
                name = "TrilinosMLSolver"
                print_info_level = 1
                max_iterations = 1
            }
          mechanicsPreconditioner{
              name = "TrilinosMLSolver"
              print_info_level = 1
              PDE_equations = 3
              max_iterations = 1
              aggregationtype = "Uncoupled"
              smoothertype = "Chebyshev"
              smoother_sweeps = 3
          }
        }
    }
}



