
Mesh{
    FileName = "cylinder270.e"
    MeshName = "cylinder"
    MeshType = "libMesh"
    dim      = 3
    x_offset = 0
    y_offset = 0
    z_offset = 0
    NumberOfElements = 270
}

NumberOfLoadingSteps = 1

nonlinearThermalBVPOperator {
  name="NonlinearBVPOperator"
    print_info_level = 1
    VolumeOperator = "DiffusionNonlinearFEOperator1"
    BoundaryOperator = "DirichletVectorCorrection1"
}

DiffusionNonlinearFEOperator1 {
  name = "DiffusionNonlinearFEOperator" 
    print_info_level = 1
    ActiveInputVariables {
      temperature = "temp"
    }
  OutputVariable = "temp"
    Freezeconcentration = TRUE
    Freezeburnup = TRUE
    PrincipalVariable = 0 

    DiffusionElement{
      name = "DiffusionNonlinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

  LocalModel = "DiffusionTransportModel1"
}

DirichletVectorCorrection1 {
  name = "DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    number_of_ids = 2
    id_0 = 2
    number_of_dofs_0 = 1
    dof_0_0 = 0
    value_0_0 = 1000.0
    id_1 = 3
    number_of_dofs_1 = 1
    dof_1_0 = 0
    value_1_0 = 1000.0
}

DiffusionTransportModel1{
  name = "DiffusionTransportModel"
    Material = "UO2_MSRZC_09"
    Property = "ThermalConductivity"
    Default_Concentration = .1
    Default_Temperature   = 300.
    Default_Burnup   = 0.0
} 

linearThermalBVPOperator {
  name = "LinearBVPOperator"
    print_info_level = 1
    VolumeOperator = "DiffusionLinearFEOperator2"
    BoundaryOperator = "DirichletMatrixCorrection2"
}

DiffusionLinearFEOperator2 {
  name = "DiffusionLinearFEOperator"
    print_info_level = 1
    reset_reuses_matrix = TRUE
    InputVariable = "temp"
    OutputVariable = "temp"
    FixedConcentration = TRUE
    FixedBurnup = TRUE

    DiffusionElement {
      name = "DiffusionLinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

  LocalModel = "DiffusionTransportModel1"
}

DirichletMatrixCorrection2 {
  name = "DirichletMatrixCorrection"
    print_info_level = 1
    skip_params = FALSE
    skip_rhs_correction = TRUE
    number_of_ids = 2
    id_0          = 2
    number_of_dofs_0 =  1
    dof_0_0          =  0
    id_1          = 3
    number_of_dofs_1 = 1
    dof_1_0 = 0
}

nonlinearMechanicsBVPOperator {
  name="NonlinearBVPOperator"
    print_info_level = 1
    VolumeOperator = "MechanicsNonlinearFEOperator3"
    BoundaryOperator = "DirichletVectorCorrection3"
}

MechanicsNonlinearFEOperator3 {
  name = "MechanicsNonlinearFEOperator"
    print_info_level = 1
    ActiveInputVariables {
      DISPLACEMENT = "disp"
        TEMPERATURE = "temp"
    }
  OutputVariable = "disp"
    RESET_REUSES_RADIAL_RETURN = TRUE
    JACOBIAN_REUSES_RADIAL_RETURN = TRUE
    FREEZE_TEMPERATURE = FALSE

    MechanicsElement {
      name="MechanicsNonlinearElement"
        USE_REDUCED_INTEGRATION = FALSE 
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }

  LocalModel = "ThermalStrainMaterialModel3"
}

DirichletVectorCorrection3 {
  name = "DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    number_of_ids = 2
    id_0 = 2
    number_of_dofs_0 = 3
    dof_0_0 = 0
    dof_0_1 = 1
    dof_0_2 = 2
    value_0_0 = 0.0
    value_0_1 = 0.0
    value_0_2 = 0.0
    id_1 = 3
    number_of_dofs_1 = 3
    dof_1_0 = 0
    dof_1_1 = 1
    dof_1_2 = 2
    value_1_0 = 0.0
    value_1_1 = 0.0
    value_1_2 = 0.0
}

ThermalStrainMaterialModel3 { 
  name="ThermalStrainMaterialModel"
    USE_MATERIALS_LIBRARY = false
    Material = "UO2_MSRZC_09"
    Youngs_Modulus = 2.08e11
    Poissons_Ratio = 0.23
    THERMAL_STRAIN_AS_SOURCE_TERM = FALSE
    THERMAL_EXPANSION_COEFFICIENT = 2.0e-6
    Default_Oxygen_Concentration = 0.1
    Default_Temperature = 300.0
    Default_Burnup = 0.0
}

linearMechanicsBVPOperator {
  name="LinearBVPOperator"
    print_info_level = 1
    VolumeOperator = "MechanicsLinearFEOperator4"
    BoundaryOperator = "DirichletMatrixCorrection4"
}

