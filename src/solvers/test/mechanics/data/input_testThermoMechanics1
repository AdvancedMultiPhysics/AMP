ThermalInitialGuess {
  name = "DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    number_of_ids = 2
    id_0 = 2
    number_of_dofs_0 = 1
    dof_0_0 = 0
    value_0_0 = 1000.0
    id_1 = 3
    number_of_dofs_1 = 1
    dof_1_0 = 0
    value_1_0 = 1000.0
}

NonlinearSolver{
  print_info_level = 7
    max_iterations = 100
    max_error = 1e-10

    absolute_tolerance = 1.0e-9
    relative_tolerance = 1.0e-6
    stepTolerance = 1.0e-10
    maximumFunctionEvals = 100
    usesJacobian=TRUE

    LinearSolver{
      print_info_level = 10
        max_iterations = 100
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-9
        relative_tolerance = 1.0e-01
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        Preconditioner{
            print_info_level = 1
            max_iterations = 1
            max_error = 1.0e-15
        }
    }
}



