Pressure_Boundary {
  name = "PressureBoundaryVectorCorrection"
    print_info_level = 1
    FE_ORDER = "FIRST"
    FE_FAMILY = "LAGRANGE"
    QRULE_TYPE = "QGAUSS"
    QRULE_ORDER = "DEFAULT"
    skip_params = FALSE
    number_of_ids = 1
    constant_pressure = TRUE
    id_0 = 101
    value_0 = 0.0
    //value_0 = -5.0e-1
}

NonlinearSolver{
  print_info_level = 2 
    max_iterations = 100
    max_error = 1e-10

    absolute_tolerance = 1.0e-15
    relative_tolerance = 1.0e-12
    stepTolerance = 1.0e-10
    maximumFunctionEvals = 1000
    usesJacobian=TRUE

    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ls_alpha 0.001"

    LinearSolver{
      print_info_level = 1
        max_iterations = 10
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-15
        relative_tolerance = 1.0e-13
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason"

        Preconditioner{
          print_info_level = 1
            PDE_equations = 3
            max_levels = 1
            max_iterations = 1
            max_error = 1.0e-15
              aggregationtype = "Uncoupled"
              smoothertype = "Chebyshev"
              smoother_sweeps = 3
        }
    }
}

