
MechanicsBVPOperator {
  name="LinearBVPOperator"
    print_info_level = 1

    VolumeOperator="MechanicsLinearFEOperator"
    BoundaryOperator="DirichletMatrixCorrection"
}

Load_Boundary {
  name="DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    number_of_ids = 2

    id_0 = 3
    number_of_dofs_0 = 1
    dof_0_0 = 0
    value_0_0 = 0.5

    id_1 = 4
    number_of_dofs_1 = 1
    dof_1_0 = 0
    value_1_0 = -0.5

}

LinearSolver{
  print_info_level = 2
    max_iterations = 10000
    max_error = 1e-10
    PDE_equations = 3
}

MechanicsMaterialModel {
        USE_MATERIALS_LIBRARY = FALSE
          Material = "UO2_MSRZC_09"
          name="IsotropicElasticModel"
          Youngs_Modulus = 3000.0
          Poissons_Ratio = 0.0
      }

MechanicsLinearFEOperator{
      name="MechanicsLinearFEOperator"
        print_info_level = 1
        reset_reuses_matrix = TRUE
        isAttachedToNonlinearOperator = FALSE
        InputVariable = "displacement"
        OutputVariable = "displacement"

        MechanicsElement {
          name="MechanicsLinearElement"
            USE_REDUCED_INTEGRATION = TRUE 
            FE_ORDER = "FIRST"  
            FE_FAMILY = "LAGRANGE"
            QRULE_TYPE = "QGAUSS"
            QRULE_ORDER = "DEFAULT"
        }
	LocalModel = MechanicsMaterialModel
    }

DirichletMatrixCorrection{
    name="DirichletMatrixCorrection"
      print_info_level = 1
      skip_params = FALSE
      skip_rhs_correction = TRUE
      number_of_ids = 2
      
      id_0 = 1
      number_of_dofs_0 = 3
      dof_0_0 = 0
      dof_0_1 = 1
      dof_0_2 = 2
      value_0_0 = 0.0
      value_0_1 = 0.0
      value_0_2 = 0.0
      
      id_1 = 2
      number_of_dofs_1 = 2
      dof_1_0 = 0
      dof_1_1 = 1
      value_1_0 = 0.0
      value_1_1 = 0.0
  }

