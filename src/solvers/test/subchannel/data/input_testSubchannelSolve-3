NonlinearSolver {
    name = "PetscSNESSolver"
    print_info_level = 1
    max_iterations = 500
    max_error = 1e-5

    absolute_tolerance = 1.0e-10
    relative_tolerance = 1.0e-8
    stepTolerance = 1.0e-15
    maximumFunctionEvals = 10000
    usesJacobian=FALSE
    enableLineSearchPreCheck=FALSE

    SNESOptions = "-snes_monitor -snes_type ls -snes_ksp_ew -snes_converged_reason -snes_ls_alpha 1.0e-4"

    LinearSolver{
        name = "PetscKrylovSolver"
        print_info_level = 0
        max_iterations = 10000
        max_error = 1e-10
        absolute_tolerance = 1.0e-16
        ksp_type = "fgmres"
        relative_tolerance = 1.0e-13
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason -ksp_max_it 10000 -ksp_rtol 0.05 -ksp_atol 1.0e-11 -ksp_gmres_restart 1000"
    }
}

Preconditioner {
	    name = "ColumnSolver"
            print_info_level = 1
            max_iterations = 1
            max_error = 1e-16
            absolute_tolerance = 1.0e-16
            relative_tolerance = 1.0e-13
            divergence_tolerance = 1.0e03
	    
            TrilinosPreconditioner{
	        name = "TrilinosMLSolver"
                print_info_level = 0
                max_iterations = 3
                max_levels = 5
                max_error = 1.0e-15
                coarse_type="Amesos-KLU"
                PDE_equations = 1
            }

            // Preconditioner for the subchannel
            SubchannelPreconditioner{
	        name = "BandedSolver"
                Type = "Banded"         // ML, Banded, None
		KL = 3
		KU = 3
                print_info_level = 0
                max_iterations = 3
                max_levels = 10
                max_error = 1.0e-15
                coarse_type="Amesos-KLU"
                PDE_equations = 2
            }

