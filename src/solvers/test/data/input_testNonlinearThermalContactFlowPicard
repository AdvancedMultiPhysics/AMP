    QRULE_ORDER = "DEFAULT"
  }
}

NonlinearSolver{
  print_info_level = 7
    max_iterations = 500
    max_error = 1e-10

    absoluteTolerance = 1.0e-10
    relativeTolerance = 1.0e-10
    stepTolerance = 1.0e-10
    maximumFunctionEvals = 100
    usesJacobian=FALSE

    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ksp_ew -snes_ls_maxstep 10000.0 -snes_ls_alpha 1.0e-4"

    LinearSolver{
      print_info_level = 0
        max_iterations = 100
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-9
        relative_tolerance = 1.0e-06
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason -ksp_max_it 100 -ksp_rtol 1.0e-9 -ksp_atol 1.0e-9"

        Preconditioner{
          print_info_level = 0
            max_iterations = 1
            max_levels = 10
            max_error = 1.0e-10
            coarse_type="Amesos-KLU"
            PDE_equations = 1
        }
    }
}

