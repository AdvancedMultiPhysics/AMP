Mesh{
    MeshName = "cube"
    MeshType = "AMP"
    dim = 3
    Generator = "cube"
    Size = 50,50,50
    Range = 0, 1, 0, 1, 0, 1
    x_offset = -0.5
    y_offset = -0.5
    z_offset = -0.5
}

LinearOperator {
  name="LinearOperator"
  MemoryLocation = "managed"
  print_info_level = 1
}

LinearSolver{
  name = "CGSolver"
  uses_preconditioner = FALSE
  absolute_tolerance = 1.0e-12
  relative_tolerance = 1.0e-12
  print_info_level = 3
  max_iterations = 100
}


