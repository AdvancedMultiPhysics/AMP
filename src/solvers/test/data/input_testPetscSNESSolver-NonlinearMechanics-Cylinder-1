}

DummyDatabase {
  print_info_level = 1
}

NonlinearSolver{
  print_info_level = 7 
    max_iterations = 100
    max_error = 1e-10

    absoluteTolerance = 1.0e-9
    relativeTolerance = 1.0e-9
    stepTolerance = 1.0e-12
    maximumFunctionEvals = 10000
    usesJacobian=FALSE

    //Change cubic to basic to turn off line search
    //Add -snes_ksp_ew if you want
    //-snes_ls_maxstep controls the max 2-norm of the update in the Newton step.
    SNESOptions = "-snes_monitor -snes_type ls -snes_ls cubic -snes_ls_maxstep 1.0e6 -snes_converged_reason -snes_ls_alpha 1.0e-4"

    LinearSolver{
      print_info_level = 1
        max_iterations = 100
        max_error = 1e-10
        ksp_type = "cg"
        absolute_tolerance = 1.0e-10
        relative_tolerance = 1.0e-10
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "LEFT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason"

        Preconditioner {
          print_info_level = 1
            PDE_equations = 3
            max_iterations = 1
            max_error = 1.0e-15
        }
    }
}


