
useConstantInitialGuess = FALSE
initialGuessScaleFactor = 1000
numFrequencies = 10

Mesh{
  dim = 3
  cube_side = 6
  xMin = 0.0
  xMax = 1.0
  yMin = 0.0
  yMax = 1.0
  zMin = 0.0
  zMax = 1.0
}

ThermalOperator{
  print_info_level = 0
  isLinearOperator = FALSE
  buildConductivityAtGaussPoints = TRUE
  useConstantConductivity = FALSE
  constantConductivityValue = 1.0
}

NonlinearSolver{
  print_info_level = 1
  max_iterations = 100
  max_error = 1e-10

  absoluteTolerance = 1.0e-9
  relativeTolerance = 1.0e-06
  stepTolerance = 1.0e-10
  maximumFunctionEvals = 100
  usesJacobian=TRUE

LinearSolver{
        print_info_level = 1
        max_iterations = 10
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-9
        relative_tolerance = 1.0e-06
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 20
        uses_preconditioner=FALSE
        pc_type = "none"
        pc_side = "RIGHT"
}


}


