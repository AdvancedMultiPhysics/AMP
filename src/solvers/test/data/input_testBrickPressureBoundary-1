
Mesh{
    FileName = "brick.e"
    MeshName = "brick"
    MeshType = "libMesh"
    dim      = 3
}

NumberOfLoadingSteps = 10

nonlinearMechanicsBVPOperator {
    name="NonlinearBVPOperator"
    print_info_level = 1
    VolumeOperator = "MechanicsNonlinearFEOperator1"
    BoundaryOperator = "DirichletVectorCorrection1"
}

MechanicsNonlinearFEOperator1 {
      name = "MechanicsNonlinearFEOperator"
        print_info_level = 1
        ActiveInputVariables {
          DISPLACEMENT = "displacement"
        }
      OutputVariable = "displacement"
        RESET_REUSES_RADIAL_RETURN = TRUE
        JACOBIAN_REUSES_RADIAL_RETURN = TRUE

        MechanicsElement {
          name="MechanicsNonlinearElement"
            USE_REDUCED_INTEGRATION = TRUE 
            FE_ORDER = "FIRST"  
            FE_FAMILY = "LAGRANGE"
            QRULE_TYPE = "QGAUSS"
            QRULE_ORDER = "DEFAULT"
        }

	LocalModel = "VonMisesElastoPlasticModel"
    }

DirichletVectorCorrection1 {
    name = "DirichletVectorCorrection"
      print_info_level = 1
      skip_params = FALSE
      number_of_ids = 1
      id_0 = 2
      number_of_dofs_0 = 3
      dof_0_0 = 0
      dof_0_1 = 1
      dof_0_2 = 2
      value_0_0 = 0.0
      value_0_1 = 0.0
      value_0_2 = 0.0
  }

Pressure_Boundary {
  name = "PressureBoundaryVectorCorrection"
    print_info_level = 1
    USE_REDUCED_INTEGRATION = FALSE
    FE_ORDER = "FIRST"
    FE_FAMILY = "LAGRANGE"
    QRULE_TYPE = "QGAUSS"
    QRULE_ORDER = "DEFAULT"
    skip_params = FALSE
    number_of_ids = 1
    constant_pressure = TRUE
    id_0 = 101
    value_0 = 2.0e5
    //value_0 = 0.0
}

VonMisesElastoPlasticModel {
        USE_MATERIALS_LIBRARY = FALSE
          print_info_level = 6
          Material = "UO2_MSRZC_09"
          name="VonMisesElastoPlasticModel"
          //name="IsotropicElasticModel"
          Youngs_Modulus = 2.08e11
          Poissons_Ratio = 0.29
          Linear_Strain_Hardening = 5.0e8
          Elastic_Yield_Stress = 1.0e7
          Default_Temperature = 310.0
          Default_Burnup = 0.0
          Default_Oxygen_Concentration = .1
      }

linearMechanicsBVPOperator {
    name="LinearBVPOperator"
    print_info_level = 1
    VolumeOperator = "MechanicsLinearFEOperator2"
    BoundaryOperator = "DirichletMatrixCorrection2"
}

MechanicsLinearFEOperator2 {
      name = "MechanicsLinearFEOperator"
        print_info_level = 1
        reset_reuses_matrix = TRUE
        InputVariable = "displacement"
        OutputVariable = "displacement"
        isAttachedToNonlinearOperator = TRUE

        MechanicsElement {
          name="MechanicsLinearElement"
            USE_REDUCED_INTEGRATION = TRUE 
            FE_ORDER = "FIRST"  
            FE_FAMILY = "LAGRANGE"
            QRULE_TYPE = "QGAUSS"
            QRULE_ORDER = "DEFAULT"
        }
    }

DirichletMatrixCorrection2 {
    name = "DirichletMatrixCorrection"
      print_info_level = 1
      skip_params = FALSE
      skip_rhs_correction = TRUE
      number_of_ids = 1
      id_0          = 2
      number_of_dofs_0 =  3
      dof_0_0          =  0
      dof_0_1          =  1
      dof_0_2          =  2
  }

Load_Boundary {
  name = "DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    number_of_ids = 1
    id_0 = 1
    number_of_dofs_0 = 1
    dof_0_0 = 2
    //value_0_0 = 2.0e7
    value_0_0 = 0.0
}

Displacement_Boundary {
  name = "DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    number_of_ids = 1
    id_0 = 2
    number_of_dofs_0 = 3
    dof_0_0 = 0
    dof_0_1 = 1
    dof_0_2 = 2
    value_0_0 = 0.0
    value_0_1 = 0.0
    value_0_2 = 0.0
}

NonlinearSolver{
  print_info_level = 7 
    max_iterations = 100
    max_error = 1e-10

    absoluteTolerance = 1.0e-10
    relativeTolerance = 1.0e-10
    stepTolerance = 1.0e-10
    maximumFunctionEvals = 1000
    usesJacobian=TRUE

    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ls_alpha 0.01"

    LinearSolver{
      print_info_level = 1
        max_iterations = 100
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-9
        relative_tolerance = 0.1
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason"

        Preconditioner{
          print_info_level = 1
            PDE_equations = 3
            max_iterations = 1
            max_error = 1.0e-15
        }
    }
}

