Pressure_Boundary {
  name = "PressureBoundaryVectorCorrection"
    print_info_level = 1
    FE_ORDER = "FIRST"  
    FE_FAMILY = "LAGRANGE"
    QRULE_TYPE = "QGAUSS"
    QRULE_ORDER = "DEFAULT"
    skip_params = FALSE
    number_of_ids = 1
    constant_pressure = TRUE
    id_0 = 1
    value_0 = 1.0e8
}

NonlinearSolver{
  print_info_level = 7 
    max_iterations = 100
    max_error = 1e-16
    absoluteTolerance = 1.0e-16
    relativeTolerance = 1.0e-10
    stepTolerance = 1.0e-15
    maximumFunctionEvals = 1000
    usesJacobian=TRUE

    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ls_alpha 0.01"

    LinearSolver{
      print_info_level = 10
        max_iterations = 1000
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-16
        relative_tolerance = 1.0e-13
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason -ksp_max_it 1000"

        Preconditioner{
          print_info_level = 1
            max_iterations = 1
            max_error = 1e-16
            absolute_tolerance = 1.0e-16
            relative_tolerance = 1.0e-13
            divergence_tolerance = 1.0e03

            TrilinosPreconditioner {
              problem_type = "SA"
                print_info_level = 10
                PDE_equations = 3
                max_iterations = 1
                max_error = 1.0e-10
            }
        }
    }
}

