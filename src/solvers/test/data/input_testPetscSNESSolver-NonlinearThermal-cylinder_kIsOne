
Mesh{
    MeshName = "cylinder"
    MeshType = "AMP"
    Generator = "tube"
    Size = 2, 132, 17
    Range = 5.46e-3, 6.35e-3, 0, 0.0127
    surfaceIds = 8, 4, -1, -1, 1, 2
    static = TRUE
}

NumberOfLoadingSteps = 1

testNonlinearThermalOperator {
  name="NonlinearBVPOperator"
    print_info_level = 2
    VolumeOperator = "DiffusionNonlinearFEOperator1"
    BoundaryOperator = "DirichletVectorCorrection1"
}

DiffusionTransportModel1{
  name = "DiffusionTransportModel"
    Material = "Independent"
    Property = "ThermalConductivity"
    Default_Concentration = .1
    Default_Temperature   = 300.
    Default_Burnup   = 0.0
} 

DiffusionNonlinearFEOperator1 {
  name = "DiffusionNonlinearFEOperator" 
    print_info_level = 1
    ActiveInputVariables {
      temperature = "temperature"
    }
  OutputVariable = "temperature"
    PrincipalVariable = "temperature" 

    DiffusionElement{
      name = "DiffusionNonlinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

  LocalModel = DiffusionTransportModel1
}

DirichletVectorCorrection1 {
  name = "DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    number_of_ids = 2
    id_0 = 4
    number_of_dofs_0 = 1
    dof_0_0 = 0
    value_0_0 = 0.
    id_1 = 8
    number_of_dofs_1 = 1
    dof_1_0 = 0
    value_1_0 = 0.
}

testLinearThermalOperator {
  name = "LinearBVPOperator"
    print_info_level = 2
    VolumeOperator = "DiffusionLinearFEOperator2"
    BoundaryOperator = "DirichletMatrixCorrection2"
}

DiffusionLinearFEOperator2 {
  name = "DiffusionLinearFEOperator"
    print_info_level = 1
    reset_reuses_matrix = TRUE
    InputVariable = "temperature"
    OutputVariable = "temperature"
    FixedConcentration = TRUE
    FixedBurnup = TRUE

    DiffusionElement {
      name = "DiffusionLinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

  LocalModel = DiffusionTransportModel1
}

DirichletMatrixCorrection2 {
  name = "DirichletMatrixCorrection"
    print_info_level = 1
    skip_params = FALSE
    skip_rhs_correction = TRUE
    number_of_ids = 2
    id_0          = 4
    number_of_dofs_0 =  1
    dof_0_0          =  0
    id_1          = 8
    number_of_dofs_1 = 1
    dof_1_0 = 0
}

ThermalInitialGuess {
  name = "DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    number_of_ids = 2
    id_0 = 4
    number_of_dofs_0 = 1
    dof_0_0 = 0
    value_0_0 = 300.0
    id_1 = 8
    number_of_dofs_1 = 1
    dof_1_0 = 0
    value_1_0 = 500.0
}

NonlinearSolver{
  print_info_level = 2
    max_iterations = 500
    max_error = 1e-10

    absolute_tolerance = 1.0e-9
    relative_tolerance = 1.0e-6
    stepTolerance = 1.0e-10
    maximumFunctionEvals = 100
    usesJacobian=FALSE

    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ksp_ew -snes_ls_alpha 0.0001"

    LinearSolver{
      print_info_level = 10
        max_iterations = 100000
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-9
        relative_tolerance = 1.0e-01
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=FALSE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason"

        Preconditioner{
          print_info_level = 1
            max_iterations = 1
            max_error = 1.0e-15
        }
    }
}



