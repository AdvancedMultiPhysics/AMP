
Mesh{
    FileName = "cube.with.12.boundaries.2.e"
    MeshName = "brick"
    MeshType = "libMesh"
    dim      = 3
    x_offset = 0
    y_offset = 0
    z_offset = 0
    NumberOfElements = 1000
}

DiffusionBVPOperator {
   name="LinearBVPOperator"
   print_info_level = 1
   VolumeOperator="DiffusionLinearFEOperator"
   BoundaryOperator="ColumnBoundaryOperator"

}

DirichletVectorCorrection {
   name="DirichletVectorCorrection"
   skip_params = FALSE
   print_info_level = 3
   number_of_ids = 3
   id_0             = 9
   id_1             = 11
   number_of_dofs_0 =   1
   number_of_dofs_1 =   1
