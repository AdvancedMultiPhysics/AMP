   dof_1_0          =   0
   value_0_0        = 300.
   value_1_0        = 300.
}

LinearSolver{
  print_info_level = 3
  max_iterations = 10
  max_error = 1e-10
  }

DiffusionLinearFEOperator{
   name="DiffusionLinearFEOperator"
   FixedTemperature = TRUE
   FixedConcentration = TRUE
   FixedBurnup   = TRUE
   InputVariable = "therm"
   OutputVariable = "therm"
   print_info_level = 3
   LocalModel = "DiffusionTransportModel"
   DiffusionElement {
     name="DiffusionLinearElement"
     print_info_level = 3
     FE_ORDER = "FIRST"  
     FE_FAMILY = "LAGRANGE"
     QRULE_TYPE = "QGAUSS"
     QRULE_ORDER = "DEFAULT"
   }
 }

ColumnBoundaryOperator{
   name="ColumnBoundaryOperator"
   numberOfBoundaryOperators=3
   boundaryOperators="RobinBoundaryOperator","NeumannVectorCorrection","DirichletMatrixCorrection"


}

