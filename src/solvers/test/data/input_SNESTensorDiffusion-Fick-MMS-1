   }

FickDiffusionLinearFEOperator{
      name="DiffusionLinearFEOperator"
      InputVariable="concentration"
      OutputVariable="divergence"
    
     DiffusionElement {
       name = "DiffusionLinearElement"
            FE_ORDER = "FIRST"  
       FE_FAMILY = "LAGRANGE"
       QRULE_TYPE = "QGAUSS"
       QRULE_ORDER = "DEFAULT"
     }

     LocalModel = "DiffusionTransportModel"
   }
   
