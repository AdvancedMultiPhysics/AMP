NonlinearSolver{
  print_info_level = 1 
    max_iterations = 100
    max_error = 1e-10

    absoluteTolerance = 1.0e-15
    relativeTolerance = 1.0e-11
    stepTolerance = 1.0e-10
    maximumFunctionEvals = 1000
    usesJacobian=TRUE

    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ls_alpha 0.0000001"

    LinearSolver{
      print_info_level = 1
        max_iterations = 10
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-15
        relative_tolerance = 1.0e-10
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason"

        Preconditioner{
          print_info_level = 1
            PDE_equations = 3
            max_levels = 1
            max_iterations = 1
            max_error = 1.0e-10
            increasingordecreasing = "increasing"
            aggregationtype = "Uncoupled"
            smoothertype = "Chebyshev"
            smoother_sweeps = 3
            smoother_preorpost = "both"
            coarse_type="Amesos-KLU"
        }
    }
}

