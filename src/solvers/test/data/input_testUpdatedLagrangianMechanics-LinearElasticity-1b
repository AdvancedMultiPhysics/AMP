        MechanicsElement {
          name="MechanicsNonlinearUpdatedLagrangianElement"
            USE_REDUCED_INTEGRATION = FALSE 
            USE_JAUMANN_RATE = FALSE
            FE_ORDER = "FIRST"  
            FE_FAMILY = "LAGRANGE"
            QRULE_TYPE = "QGAUSS"
            QRULE_ORDER = "DEFAULT"
        }

	LocalModel = "VonMisesElastoPlasticModel1"
}

