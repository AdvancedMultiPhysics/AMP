
Mesh{
    FileName = "brick.e"
    MeshName = "brick"
    MeshType = "libMesh"
    dim      = 3
    x_offset = 0
    y_offset = 0
    z_offset = 0
    NumberOfElements = 1088
}

NumberOfLoadingSteps = 1

nonlinearMechanicsBVPOperator {
  name="NonlinearBVPOperator"
    print_info_level = 1

    VolumeOperator = "MechanicsNonlinearFEOperator"
    BoundaryOperator = "DirichletVectorCorrection"
}

linearMechanicsBVPOperator {
  name="LinearBVPOperator"
    print_info_level = 1

    VolumeOperator = "MechanicsLinearFEOperator"
    BoundaryOperator = "DirichletMatrixCorrection"
}

Load_Boundary {
  name = "DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    number_of_ids = 1
    id_0 = 1
    number_of_dofs_0 = 1
    dof_0_0 = 2
    value_0_0 = 10.0
}

Displacement_Boundary {
  name = "DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    number_of_ids = 1
    id_0 = 2
    number_of_dofs_0 = 3
    dof_0_0 = 0
    dof_0_1 = 1
    dof_0_2 = 2
    value_0_0 = 0.0
    value_0_1 = 0.0
    value_0_2 = 0.0
}

NonlinearSolver{
  print_info_level = 7
    max_iterations = 100
    max_error = 1e-10
    max_vectors = 10
    angle_tolerance = 0.2
    freeze_pc = FALSE

    absoluteTolerance = 1.0e-9
    relativeTolerance = 1.0e-6
    stepTolerance = 1.0e-10
    maximumFunctionEvals = 100
    usesJacobian=TRUE

    LinearSolver{
      print_info_level = 10
        max_iterations = 100
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-9
        relative_tolerance = 1.0e-01
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        Preconditioner{
          print_info_level = 10
            PDE_equations = 3
            max_iterations = 1
            max_error = 1.0e-15
        }
    }
}

MechanicsNonlinearFEOperator{
  name = "MechanicsNonlinearFEOperator"
    print_info_level = 1
    ActiveInputVariables {
      DISPLACEMENT = "displacement"
    }
  OutputVariable = "displacement"
    RESET_REUSES_RADIAL_RETURN = TRUE
    JACOBIAN_REUSES_RADIAL_RETURN = TRUE

    MechanicsElement {
      name="MechanicsNonlinearElement"
        USE_REDUCED_INTEGRATION = FALSE 
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }
  LocalModel = "MechanicsMaterialModel"

}

DirichletVectorCorrection{
  name = "DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    number_of_ids = 1
    id_0 = 2
    number_of_dofs_0 = 3
    dof_0_0 = 0
    dof_0_1 = 1
    dof_0_2 = 2
    value_0_0 = 0.0
    value_0_1 = 0.0
    value_0_2 = 0.0
}

MechanicsLinearFEOperator{
  name = "MechanicsLinearFEOperator"
    print_info_level = 1
    reset_reuses_matrix = TRUE
    InputVariable = "displacement"
    OutputVariable = "displacement"

    MechanicsElement {
      name="MechanicsLinearElement"
        USE_REDUCED_INTEGRATION = FALSE 
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }
}

DirichletMatrixCorrection{
  name = "DirichletMatrixCorrection"
    print_info_level = 1
    skip_params = FALSE
    skip_rhs_correction = TRUE
    number_of_ids = 1
    id_0          = 2
    number_of_dofs_0 =  3
    dof_0_0          =  0
    dof_0_1          =  1
    dof_0_2          =  2
}


MechanicsMaterialModel {
  USE_MATERIALS_LIBRARY = FALSE
    Material = "UO2_MSRZC_09"
    name="VonMisesElastoPlasticModel"
    Youngs_Modulus = 2.08e11
    Poissons_Ratio = 0.23
    Linear_Strain_Hardening = 2.10e9
    Elastic_Yield_Stress = 1.45e8
    Default_Temperature = 310.0
    Default_Burnup = 0.0
    Default_Oxygen_Concentration = .1
}
