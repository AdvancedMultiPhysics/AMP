SNES_EW_JFNK {
  print_info_level = 0 
    max_iterations = 30
    max_error = 1e-10
    absoluteTolerance = 1.0e-10
    relativeTolerance = 1.0e-11
    stepTolerance = 1.0e-12
    maximumFunctionEvals = 1000
    zero_initial_guess = FALSE
    usesJacobian = FALSE

    SNESOptions = "-snes_monitor -snes_converged_reason -snes_ksp_ew -snes_type ls -snes_ls_alpha 0.0001"

    LinearSolver {
      print_info_level = 0
        max_iterations = 20
        max_error = 1.0e-30
        ksp_type = "cg"
        absolute_tolerance = 1.0e-30
        relative_tolerance = 1.0e-12
        divergence_tolerance = 1.0e03
        uses_preconditioner = TRUE
        pc_type = "shell"
        pc_side = "LEFT"
        KSPOptions = "-ksp_monitor_cancel -ksp_monitor_true_residual -ksp_converged_reason -ksp_max_it 20"
    }
}

