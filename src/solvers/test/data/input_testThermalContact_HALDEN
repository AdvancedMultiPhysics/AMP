    numTimeSteps       = 1
    timeSteps          = .1
    useFixedValue      = TRUE
    fixedValues        = 4045.83
    print_info_level   = 3
}

LinearThermalOperator1 {
  name = "LinearBVPOperator"
    print_info_level = 3
    VolumeOperator = "DiffusionLinearFEOperator1"
    BoundaryOperator = "RobinMatrixCorrection1"
}

DiffusionLinearFEOperator1 {
  name = "DiffusionLinearFEOperator"
    print_info_level = 1
    reset_reuses_matrix = TRUE
    InputVariable = "Temperature"
    OutputVariable = "Temperature"
    FixedConcentration = TRUE
    FixedBurnup = TRUE

    DiffusionElement {
      name = "DiffusionLinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

  LocalModel = "DiffusionTransportModel1"
}

