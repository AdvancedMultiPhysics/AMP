NonlinearSolver {
  max_iterations = 50
    max_error = 1e-12
    zero_initial_guess = FALSE
    absoluteTolerance = 1.0e-12
    relativeTolerance = 1.0e-10
    stepTolerance = 1.0e-14
    maximumFunctionEvals = 1000
    usesJacobian = FALSE
    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ls_alpha 1.0e-4"

    LinearSolver {
      max_iterations = 100
        max_error = 1e-12
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-12
        relative_tolerance = 1.0e-11
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner = TRUE
        pc_type = "shell"
        pc_side = "RIGHT"
        KSPOptions = "-ksp_monitor -ksp_converged_reason -ksp_max_it 100"

        PelletStackSolver {
          ColumnSolver {
            max_iterations = 1
              max_error = 1e-12

              KrylovSolver {
                max_iterations = 100
                  max_error = 1e-12
                  ksp_type = "fgmres"
                  absolute_tolerance = 1.0e-12
                  relative_tolerance = 1.0e-11
                  divergence_tolerance = 1.0e03
                  max_krylov_dimension = 40
                  uses_preconditioner = TRUE
                  pc_type = "shell"
                  pc_side = "RIGHT"
                  KSPAppendOptionsPrefix = "kspPC_"
                  KSPOptions = "-kspPC_ksp_max_it 100 -kspPC_ksp_converged_reason -kspPC_ksp_monitor" 

                  MLSolver {
                    max_iterations = 1
                      max_error = 1e-11
                      problem_type = "NSSA"
                      PDE_equations = 3
                      // Optional, but it makes the first iteration converge very tightly, very quickly.
                      null_space_type = "pre-computed"
                      null_space_dimension = 6
                      null_space_add_default_vectors = FALSE
                  }
              }
          }
        }
    }
}





