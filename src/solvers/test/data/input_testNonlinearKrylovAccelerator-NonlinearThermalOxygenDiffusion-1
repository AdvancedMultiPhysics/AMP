NeutronicsOperator{
  name="NeutronicsRhsOperator"
    type               = "Power"
    numTimeSteps       = 2
    timeSteps          = 1, 10000.
    useFixedValue      = TRUE
    fixedValues        = 1000.0, 2.0
    print_info_level   = 3
}

VolumeIntegralOperator{
  name="VolumeIntegralOperator"
    ConstantSource   = FALSE
    InputVariable = "therm"
    OutputVariable = "therm"
    print_info_level = 0

    SourceElement {
      name="SourceNonlinearElement"
        print_info_level = 3
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }
}

NumberOfLoadingSteps = 1

testNonlinearThermalOperator {
  name="NonlinearBVPOperator"
    print_info_level = 0


    VolumeOperator = "DiffusionNonlinearFEOperator1"
    BoundaryOperator = "DirichletVectorCorrection1"
}
DiffusionNonlinearFEOperator1 {
  name = "DiffusionNonlinearFEOperator" 
    print_info_level = 0
    ActiveInputVariables {
      temperature = "temp"
        concentration = "concentration"
    }
  OutputVariable = "temp"
    PrincipalVariable = 0 

    DiffusionElement{
      name = "DiffusionNonlinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

  LocalModel = "DiffusionTransportModel1"
}
DirichletVectorCorrection1 {
  name = "DirichletVectorCorrection"
    print_info_level = 0
    skip_params = FALSE
    number_of_ids = 2
    id_0 = 2
    number_of_dofs_0 = 1
    dof_0_0 = 0
    value_0_0 = 0.0
    id_1 = 3
    number_of_dofs_1 = 1
    dof_1_0 = 0
    value_1_0 = 0.0
}
DiffusionTransportModel1{
  name = "DiffusionTransportModel"
    Material = "UO2_MSRZC_09"
    Property = "ThermalConductivity"
    Default_Concentration = .1
    Default_Temperature   = 300.
    Default_Burnup   = 0.0
} 

