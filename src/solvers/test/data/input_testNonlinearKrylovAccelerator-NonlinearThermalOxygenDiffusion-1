  name = "DirichletVectorCorrection"
    print_info_level = 0
    skip_params = FALSE
    number_of_ids = 2
    id_0 = 2
    number_of_dofs_0 = 1
    dof_0_0 = 0
    value_0_0 = 0.1
    id_1 = 3
    number_of_dofs_1 = 1
    dof_1_0 = 0
    value_1_0 = 0.1
}

NonlinearSolver{
  print_info_level = 1
    max_iterations = 1000
    max_error = 1e-10
    max_vectors = 5
    angle_tolerance = 0.2
    freeze_pc = FALSE

    absoluteTolerance = 1.0e-16
    relativeTolerance = 1.0e-16
    stepTolerance = 1.0e-16
    maximumFunctionEvals = 100
    usesJacobian=FALSE

    LinearSolver{
      print_info_level = 0
        max_iterations = 100
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-9
        relative_tolerance = 0.1
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        Preconditioner{
          print_info_level = 0
            max_iterations = 1
            max_error = 1e-10
            absolute_tolerance = 1.0e-16
            relative_tolerance = 1.0e-01
            divergence_tolerance = 1.0e03

            thermalPreconditioner{
              print_info_level = 0
                max_iterations = 1
                max_error = 1.0e-15
            }
          oxygenPreconditioner{
            print_info_level = 0
              max_iterations = 1
              max_error = 1.0e-15
          }
        }
    }
}



