// Database for the meshes
Mesh{
    // This is the main multimesh that will contain all meshes
    MeshName = "cylinder"
    MeshType = "libMesh"
    FileName = "halden_1_pellet_9mil.e"
    dim = 3
    x_offset = 0.0
    y_offset = 0.0
    z_offset = 0.0
    NumberOfElements = 5226
}

testNonlinearFickSoretBVPOperator {
  name="NonlinearBVPOperator"
  print_info_level = 3
  VolumeOperator = "FickSoretNonlinearFEOperator1"
  BoundaryOperator = "DirichletVectorCorrection1"
}
  FickSoretNonlinearFEOperator1 {
	name = "FickSoretNonlinearFEOperator"
	AddSoretTerm = TRUE

        FickOperator="FickDiffusionNonlinearFEOperator"

       SoretOperator="SoretDiffusionNonlinearFEOperator"
  }

FickDiffusionNonlinearFEOperator{
      name = "DiffusionNonlinearFEOperator" 
        print_info_level = 3
        ActiveInputVariables {
          temperature = "temp"
          concentration = "conc"
        }
        Freezetemperature = TRUE
        OutputVariable = "conc"
        PrincipalVariable = 1

        DiffusionElement{
          name = "DiffusionNonlinearElement"
            TransportAtGaussPoints = TRUE
            FE_ORDER = "FIRST"  
            FE_FAMILY = "LAGRANGE"
            QRULE_TYPE = "QGAUSS"
            QRULE_ORDER = "DEFAULT"
        }    
	LocalModel = "FickDiffusionTransportModel"
    }
    
SoretDiffusionNonlinearFEOperator{
      name = "DiffusionNonlinearFEOperator" 

      print_info_level = 3    
      ActiveInputVariables {
        temperature = "temp"
        concentration = "conc"
      }
      Freezetemperature = TRUE
      OutputVariable = "conc"
      PrincipalVariable = 0 

      DiffusionElement{
        name = "DiffusionNonlinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
      }    
      LocalModel = "SoretDiffusionTransportModel"
    }

DirichletVectorCorrection1 {
    name = "DirichletVectorCorrection"
    print_info_level = 3
    skip_params = FALSE
    isAttachedToVolumeOperator = TRUE
    number_of_ids = 1
    id_0             = 4
    number_of_dofs_0 =   1
    dof_0_0          =   0
    value_0_0        = .15
  }

testLinearFickBVPOperator {
    name = "LinearBVPOperator"
    print_info_level = 3
    VolumeOperator = "DiffusionLinearFEOperator2"
    BoundaryOperator = "DirichletMatrixCorrection2"
}

DiffusionLinearFEOperator2 {
        name = "DiffusionLinearFEOperator"
        print_info_level = 0
        reset_reuses_matrix = TRUE
        InputVariable = "conc"
        OutputVariable = "conc"
        FixedTemperature = TRUE
        FixedBurnup = TRUE

        DiffusionElement {
          name = "DiffusionLinearElement"
            TransportAtGaussPoints = TRUE
            FE_ORDER = "FIRST"  
            FE_FAMILY = "LAGRANGE"
            QRULE_TYPE = "QGAUSS"
            QRULE_ORDER = "DEFAULT"
        }    

	LocalModel = "FickDiffusionTransportModel"
    }
    
DirichletMatrixCorrection2 {
      name = "DirichletMatrixCorrection"
      print_info_level = 0
      skip_params = FALSE
      skip_rhs_correction = TRUE
      number_of_ids = 1
      id_0          = 4
      number_of_dofs_0 =  1
      dof_0_0          =  0
      value_0_0	       =  0
    }

NonlinearSolver{
    print_info_level = 7
    max_iterations = 500
    max_error = 1e-10

    absolute_tolerance = 1.0e-10
    relative_tolerance = 1.0e-10
    stepTolerance = 1.0e-10
    maximumFunctionEvals = 100
    usesJacobian=FALSE

    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ksp_ew -snes_ls_alpha 0.0001"

    LinearSolver{
      print_info_level = 0
      max_iterations = 100
      max_error = 1e-10
      ksp_type = "fgmres"
      absolute_tolerance = 1.0e-10
      relative_tolerance = 1.0e-01
      divergence_tolerance = 1.0e03
      max_krylov_dimension = 40
      uses_preconditioner=TRUE
      pc_type = "shell"
      pc_side = "RIGHT"

      KSPOptions = "-ksp_monitor -ksp_converged_reason -ksp_max_it 100"

      Preconditioner{
        print_info_level = 0
        max_iterations = 1
        max_error = 1.0e-15
      }
    }
}

FickDiffusionTransportModel{
        name = "DiffusionTransportModel"
          Material = "Independent"
          Property = "FickCoefficient"
          Default_Concentration = .05
          Default_Temperature   = 300.
          Default_Burnup   = 0.0
          Parameters = 1.0
      } 

SoretDiffusionTransportModel{
        name = "DiffusionTransportModel"
        Material = "Independent"
        Property = "ThermalDiffusionCoefficient"
        Default_Concentration = .05
        Default_Temperature   = 300.
        Default_Burnup   = 0.0
        Parameters = 1.0, 0.0
      } 
