testLinearOxygenOperator {
  name = "LinearBVPOperator"
    print_info_level = 1
    VolumeOperator = "DiffusionLinearFEOperator4"
    BoundaryOperator = "DirichletMatrixCorrection4"
}

DiffusionLinearFEOperator4 {
  name = "DiffusionLinearFEOperator"
    print_info_level = 1
    reset_reuses_matrix = TRUE
    InputVariable = "concentration"
    OutputVariable = "concentration"
    FixedTemperature = TRUE
    FixedBurnup = TRUE

    DiffusionElement {
      name = "DiffusionLinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

  LocalModel = "DiffusionTransportModel3"
}

DirichletMatrixCorrection4 {
  name = "DirichletMatrixCorrection"
    print_info_level = 1
    skip_params = FALSE
    skip_rhs_correction = TRUE
    number_of_ids = 2
    id_0          = 2
    number_of_dofs_0 =  1
    dof_0_0          =  0
    id_1          = 3
    number_of_dofs_1 = 1
    dof_1_0 = 0
}

NonlinearSolver{
  print_info_level = 7
    max_iterations = 100
    max_error = 1e-10

    absolute_tolerance = 1.0e-16
    relativeTolerance = 1.0e-16
    stepTolerance = 1.0e-16
    maximumFunctionEvals = 100
    usesJacobian=FALSE

    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ksp_ew -snes_ls_alpha 0.0001 -mat_mffd_check_positivity"

    LinearSolver{
      print_info_level = 10
        max_iterations = 100
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-9
        relative_tolerance = 0.1
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason -ksp_max_it 100"

        Preconditioner{
          print_info_level = 10
            max_iterations = 1
            max_error = 1e-10
            absolute_tolerance = 1.0e-16
            relative_tolerance = 1.0e-01
            divergence_tolerance = 1.0e03

            thermalPreconditioner{
              print_info_level = 10
                max_iterations = 1
                max_error = 1.0e-15
            }

          oxygenPreconditioner{
            print_info_level = 10
              max_iterations = 1
              max_error = 1.0e-15
          }
        }
    }
}



