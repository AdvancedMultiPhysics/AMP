
Mesh{
    FileName = "cylinder4K.e"
    MeshName = "cylinder"
    MeshType = "libMesh"
    dim      = 3
    x_offset = 0
    y_offset = 0
    z_offset = 0
    NumberOfElements = 4088
}

testNonlinearThermalOperator {
  name="NonlinearBVPOperator"
    print_info_level = 1
    VolumeOperator = "DiffusionNonlinearFEOperator1"
    BoundaryOperator = "DirichletVectorCorrection1"
}

DiffusionNonlinearFEOperator1 {
  name = "DiffusionNonlinearFEOperator" 
    print_info_level = 1
    ActiveInputVariables {
      temperature = "temp"
        concentration="concentration"
    }
  OutputVariable = "temp"
    PrincipalVariable = 0 

    DiffusionElement{
      name = "DiffusionNonlinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

  LocalModel = "DiffusionTransportModel1"
}

DirichletVectorCorrection1 {
  name = "DirichletVectorCorrection"
    print_info_level = 1
    skip_params = FALSE
    isAttachedToVolumeOperator = TRUE
    number_of_ids = 2
    id_0 = 2
    number_of_dofs_0 = 1
    dof_0_0 = 0
    value_0_0 = 950.0
    id_1 = 3
    number_of_dofs_1 = 1
    dof_1_0 = 0
    value_1_0 = 750.0
}

DiffusionTransportModel1{
  name = "DiffusionTransportModel"
    Material = "UO2_MSRZC_09"
    Property = "ThermalConductivity"
    Default_Concentration = .1
    Default_Temperature   = 750.
    Default_Burnup   = 0.0
    UseBilogScaling = TRUE
    BilogVariable = "concentration"
    BilogScaleCoefficient = FALSE
} 

testLinearThermalOperator {
  name = "LinearBVPOperator"
    print_info_level = 1
    VolumeOperator = "DiffusionLinearFEOperator2"
    BoundaryOperator = "DirichletMatrixCorrection2"
}

DiffusionLinearFEOperator2 {
  name = "DiffusionLinearFEOperator"
    print_info_level = 1
    reset_reuses_matrix = TRUE
    InputVariable = "temp"
    OutputVariable = "temp"
    FixedConcentration = TRUE
    FixedBurnup = TRUE

    DiffusionElement {
      name = "DiffusionLinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

  LocalModel = "DiffusionTransportModel1"
}

DirichletMatrixCorrection2 {
  name = "DirichletMatrixCorrection"
    print_info_level = 1
    skip_params = FALSE
    skip_rhs_correction = TRUE
    number_of_ids = 2
    id_0          = 2
    number_of_dofs_0 =  1
    dof_0_0          =  0
    id_1          = 3
    number_of_dofs_1 = 1
    dof_1_0 = 0
}

testNonlinearOxygenOperator {
  name="NonlinearBVPOperator"
    print_info_level = 1
    VolumeOperator = "DiffusionNonlinearFEOperator3"
    BoundaryOperator = "DirichletVectorCorrection3"
}

