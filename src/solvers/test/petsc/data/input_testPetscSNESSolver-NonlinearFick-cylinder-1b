
Mesh{
    FileName = "halden_1_pellet_9mil.e"
    MeshType = "libMesh"
    MeshName = "Pellet"
    dim      = 3
    x_offset = 0
    y_offset = 0
    z_offset = 0
    NumberOfElements = 5226
}

testNonlinearFickOperator {
    name="NonlinearBVPOperator"
    print_info_level = 0
    VolumeOperator = "DiffusionNonlinearFEOperator1"
    BoundaryOperator = "DirichletVectorCorrection1"
}

DiffusionNonlinearFEOperator1 {
      name = "DiffusionNonlinearFEOperator" 
        print_info_level = 0
        ActiveInputVariables {
          concentration = "conc"
        }
      OutputVariable = "conc"
        PrincipalVariable = 1

        DiffusionElement{
          name = "DiffusionNonlinearElement"
            TransportAtGaussPoints = TRUE
            FE_ORDER = "FIRST"  
            FE_FAMILY = "LAGRANGE"
            QRULE_TYPE = "QGAUSS"
            QRULE_ORDER = "DEFAULT"
        }    

	LocalModel = "DiffusionTransportModel1"
    }

DirichletVectorCorrection1 {
    name = "DirichletVectorCorrection"
      print_info_level = 0
      skip_set_boundary_ids = FALSE
      isAttachedToVolumeOperator = TRUE
      number_of_ids = 2
      id_0             = 1
      number_of_dofs_0 =   1
      dof_0_0          =   0
      value_0_0        = .15
      id_1             = 2
      number_of_dofs_1 =   1
      dof_1_0          =   0
      value_1_0        = .05
  }

DiffusionTransportModel1{
        name = "DiffusionTransportModel"
          Material = "Ox_MSRZC_09"
          Property = "FickCoefficient"
          Default_Concentration = .05
          Default_Temperature   = 1350.
          Default_Burnup   = 0.0
      } 

testLinearFickOperator {
    name = "LinearBVPOperator"
    print_info_level = 0
    VolumeOperator = "DiffusionLinearFEOperator2"
    BoundaryOperator = "DirichletMatrixCorrection2"
}

DiffusionLinearFEOperator2 {
      name = "DiffusionLinearFEOperator"
        print_info_level = 0
        reset_reuses_matrix = TRUE
        InputVariable = "conc"
        OutputVariable = "conc"
        FixedTemperature = TRUE
        FixedBurnup = TRUE

        DiffusionElement {
          name = "DiffusionLinearElement"
            TransportAtGaussPoints = TRUE
            FE_ORDER = "FIRST"  
            FE_FAMILY = "LAGRANGE"
            QRULE_TYPE = "QGAUSS"
            QRULE_ORDER = "DEFAULT"
        }    

	LocalModel = "DiffusionTransportModel1"
    }

DirichletMatrixCorrection2 {
    name = "DirichletMatrixCorrection"
      print_info_level = 0
      skip_set_boundary_ids = FALSE
      skip_rhs_correction = TRUE
      number_of_ids = 2
      id_0             = 1
      number_of_dofs_0 =   1
      dof_0_0          =   0
      value_0_0        = .15
      id_1             = 2
      number_of_dofs_1 =   1
      dof_1_0          =   0
      value_1_0        = .05
  }

NonlinearSolver{
  print_info_level = 7
    max_iterations = 500
    max_error = 1e-10

    absolute_tolerance = 1.0e-13
    relativeTolerance = 1.0e-10
    stepTolerance = 1.0e-10
    maximumFunctionEvals = 100
    usesJacobian=FALSE

    SNESOptions = "-snes_monitor -snes_type ls -snes_converged_reason -snes_ksp_ew -snes_ls_alpha 0.0001"

    LinearSolver{
      print_info_level = 0
        max_iterations = 100
        max_error = 1e-10
        ksp_type = "fgmres"
        absolute_tolerance = 1.0e-13
        relative_tolerance = 1.0e-01
        divergence_tolerance = 1.0e03
        max_krylov_dimension = 40
        uses_preconditioner=TRUE
        pc_type = "shell"
        pc_side = "RIGHT"

        KSPOptions = "-ksp_monitor -ksp_converged_reason -ksp_max_it 100"

        Preconditioner{
          print_info_level = 0
            max_iterations = 1
            max_error = 1.0e-15
        }
    }
}



