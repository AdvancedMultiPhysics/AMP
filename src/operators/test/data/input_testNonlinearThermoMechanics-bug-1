
Mesh{
  MeshType = "libMesh"
  FileName = "cylinder270.e"
  MeshName = "cylinder"
  dim      = 3
  x_offset = 0.0
  y_offset = 0.0
  z_offset = 0.0
  NumberOfElements = 270
}

NumberOfLoadingSteps = 1

testNonlinearThermalOperator {
   name="NonlinearBVPOperator"
   print_info_level = 1
  
   VolumeOperator = "DiffusionNonlinearFEOperator"
   BoundaryOperator = "ThermalDirichletVectorCorrection"
}

testLinearThermalOperator {
   name = "LinearBVPOperator"
   print_info_level = 1

   VolumeOperator = "DiffusionLinearFEOperator"
   BoundaryOperator = "ThermalDirichletMatrixCorrection"
}

testNonlinearMechanicsOperator {
   name="NonlinearBVPOperator"
   print_info_level = 1
   
   VolumeOperator = "MechanicsNonlinearFEOperator"
   BoundaryOperator = "MechanicsDirichletVectorCorrection"
}

testLinearMechanicsOperator {
   name="LinearBVPOperator"
   print_info_level = 1
   
   VolumeOperator = "MechanicsLinearFEOperator"
   BoundaryOperator = "MechanicsDirichletMatrixCorrection"
}

DiffusionNonlinearFEOperator{
     name = "DiffusionNonlinearFEOperator" 
     print_info_level = 1
     ActiveInputVariables {
         temperature = "temp"
     }
     OutputVariable = "temp"
     PrincipalVariable = 0 

     DiffusionElement{
        name = "DiffusionNonlinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
     }    

     LocalModel = "DiffusionTransportModel"

   }

ThermalDirichletVectorCorrection{
      name = "DirichletVectorCorrection"
      print_info_level = 1
      skip_params = FALSE
      number_of_ids = 2
      id_0 = 2
      number_of_dofs_0 = 1
      dof_0_0 = 0
      value_0_0 = 1000.0
      id_1 = 3
      number_of_dofs_1 = 1
      dof_1_0 = 0
      value_1_0 = 1000.0
   }

DiffusionLinearFEOperator{
     name = "DiffusionLinearFEOperator"
     print_info_level = 1
     reset_reuses_matrix = TRUE
     InputVariable = "temp"
     OutputVariable = "temp"
     FixedConcentration = TRUE
     FixedBurnup = TRUE

     DiffusionElement {
        name = "DiffusionLinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
     }    
   }

ThermalDirichletMatrixCorrection{
     name = "DirichletMatrixCorrection"
     print_info_level = 1
     skip_params = FALSE
     skip_rhs_correction = TRUE
     number_of_ids = 2
     id_0          = 2
     number_of_dofs_0 =  1
     dof_0_0          =  0
     id_1          = 3
     number_of_dofs_1 = 1
     dof_1_0 = 0
   }

DiffusionTransportModel{
        name = "DiffusionTransportModel"
        Material = "UO2_MSRZC_09"
        Property = "ThermalConductivity"
        Default_Concentration = .1
        Default_Temperature   = 300.
        Default_Burnup   = 0.0
     } 

MechanicsNonlinearFEOperator{
     name = "MechanicsNonlinearFEOperator"
     print_info_level = 1
     ActiveInputVariables {
         DISPLACEMENT = "disp"
         TEMPERATURE = "temp"
     }
     OutputVariable = "disp"
     RESET_REUSES_RADIAL_RETURN = TRUE
     JACOBIAN_REUSES_RADIAL_RETURN = TRUE
     FREEZE_TEMPERATURE = FALSE

     MechanicsElement {
       name="MechanicsNonlinearElement"
       USE_REDUCED_INTEGRATION = FALSE 
       FE_ORDER = "FIRST"  
       FE_FAMILY = "LAGRANGE"
       QRULE_TYPE = "QGAUSS"
       QRULE_ORDER = "DEFAULT"
     }

     LocalModel = "MechanicsMaterialModel"
   }

MechanicsDirichletVectorCorrection{
     name = "DirichletVectorCorrection"
     print_info_level = 1
     skip_params = FALSE
     number_of_ids = 2
     id_0 = 2
     number_of_dofs_0 = 3
     dof_0_0 = 0
     dof_0_1 = 1
     dof_0_2 = 2
     value_0_0 = 0.0
     value_0_1 = 0.0
     value_0_2 = 0.0
     id_1 = 3
     number_of_dofs_1 = 3
     dof_1_0 = 0
     dof_1_1 = 1
     dof_1_2 = 2
     value_1_0 = 0.0
     value_1_1 = 0.0
     value_1_2 = 0.0
   }

MechanicsLinearFEOperator{
     name = "MechanicsLinearFEOperator"
     print_info_level = 1
     reset_reuses_matrix = TRUE
     InputVariable = "disp"
     OutputVariable = "disp"

     MechanicsElement {
       name="MechanicsLinearElement"
       USE_REDUCED_INTEGRATION = FALSE 
       FE_ORDER = "FIRST"  
       FE_FAMILY = "LAGRANGE"
       QRULE_TYPE = "QGAUSS"
       QRULE_ORDER = "DEFAULT"
     }
   }

MechanicsDirichletMatrixCorrection{
     name = "DirichletMatrixCorrection"
     print_info_level = 1
     skip_params = FALSE
     skip_rhs_correction = TRUE
     number_of_ids = 2
     id_0          = 2
     number_of_dofs_0 =  3
     dof_0_0          =  0
     dof_0_1          =  1
     dof_0_2          =  2
     id_1          = 3
     number_of_dofs_1 = 3
     dof_1_0 = 0
     dof_1_1 = 1
     dof_1_2 = 2
   }

MechanicsMaterialModel { 
      name="ThermalStrainMaterialModel"
      USE_MATERIALS_LIBRARY = FALSE
      Material = "UO2_MSRZC_09"
      Youngs_Modulus = 2.08e11
      Poissons_Ratio = 0.23
      THERMAL_STRAIN_AS_SOURCE_TERM = FALSE
      THERMAL_EXPANSION_COEFFICIENT = 2.0e-6
      Default_Oxygen_Concentration = .1
      Default_Temperature = 300.0
      Default_Burnup = 0.0
     }


