DiffusionTransportModel{
        name = "DiffusionCylindricalTransportModel"
        Material = "RadialPolynomial"
        Property = "FickCoefficient"
 		RadiusArgument = "radius"
 		// coefficients for the polynomial 1. + 1000 r (1 - r) (r - .2) (r - .52) (r - .63) (r - .8)
        Parameters =  1., 52.416, -564.016, 2149.2, -3787.6, 3150., -1000.
} 
	
