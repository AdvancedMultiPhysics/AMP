
Mesh{
  FileName = "multiElementMesh.e"
  MeshName = "ida"
  MeshType = "libMesh"
  dim      = 3        
  x_offset = 0.0
  y_offset = 0.0
  z_offset = 0.0
  NumberOfElements = 1000
}

LinearOperator {
   name="LinearBVPOperator"
   print_info_level = 4
   VolumeOperator = "DiffusionLinearFEOperator"
   BoundaryOperator="DirichletMatrixCorrection"
}

DiffusionLinearFEOperator{
    name = "DiffusionLinearFEOperator"
    InputVariable = "temperature"
    OutputVariable = "temperature"
    print_info_level = 1
    reset_reuses_matrix = TRUE
    FixedConcentration = TRUE
    FixedBurnup = TRUE

    DiffusionElement {
        name = "DiffusionLinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    
 
    LocalModel = "DiffusionTransportModel"
}

DiffusionTransportModel{
        name = "DiffusionTransportModel"
        Property = "ThermalConductivity"
        Material = "UO2_MSRZC_09"
        Default_Concentration = .1
        Default_Temperature   = 750.
        Default_Burnup   = 0.0
        BilogScaleCoefficient = FALSE
} 

DirichletMatrixCorrection{
    name="DirichletMatrixCorrection"
    skip_params = FALSE
    skip_rhs_correction = FALSE 
    skip_rhs_add_correction = FALSE 
    print_info_level = 3
    number_of_ids = 1
    id_0             = 10
    number_of_dofs_0 =   1
    dof_0_0          =   0
    value_0_0        = 750.
}

MassLinearOperator {
   name="LinearBVPOperator"
   print_info_level = 3
   VolumeOperator = "MassLinearFEOperator"
   BoundaryOperator="MassMatrixCorrection"
}

MassLinearFEOperator {
    name = "MassLinearFEOperator"
    InputVariable = "temperature"
    OutputVariable = "temperature"
    print_info_level = 3
    reset_reuses_matrix = TRUE
    FixedConcentration = 0.15
    FixedTemperature   = 750
    FixedBurnup   = 0.0


    MassElement {
        name = "MassLinearElement"
		print_info_level = 3
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

    LocalModel = "MassDensityModel"
}

MassDensityModel{
        name = "MassDensityModel"
        Material = "UO2_MSRZC_09"
        Equation = "Thermal"
        Property = "ThermalConductivity"
        Default_Concentration = .1
        Default_Temperature   = 750.
        Default_Burnup   = 0.0
        BilogScaleCoefficient = FALSE
	print_info_level = 3
}

MassMatrixCorrection{
  name="MassMatrixCorrection"
    skip_params = FALSE
    skip_rhs_correction = TRUE
    skip_rhs_add_correction = TRUE
    print_info_level = 3
    number_of_ids = 1
    id_0             = 10
    number_of_dofs_0 =   1
    dof_0_0          =   0
    value_0_0        = 0.
}

