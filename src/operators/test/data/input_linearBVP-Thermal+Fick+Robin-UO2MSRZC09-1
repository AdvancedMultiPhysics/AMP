   FE_ORDER = "FIRST"
   FE_FAMILY = "LAGRANGE"
   QRULE_TYPE = "QGAUSS"
   QRULE_ORDER = "DEFAULT"
   fConductivity  = 1.0
   alpha          = 1.0
   beta           = 1.0
   gamma          = 1.0
   print_info_level = 3
   number_of_ids    = 1
   id_0             = 2
   number_of_dofs_0 =   1
   dof_0_0          =   0
   value_0_0        = 101.
  }


