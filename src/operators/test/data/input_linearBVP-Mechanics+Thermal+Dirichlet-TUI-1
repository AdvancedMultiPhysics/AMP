numberOfOperators=2

testOperator1 {
   name="LinearBVPOperator"
   print_info_level = 1

   VolumeOperator = "MechanicsLinearFEOperator"
   BoundaryOperator="MechanicsDirichletMatrixCorrection"
}

testOperator2 {
   name="LinearBVPOperator"
   print_info_level = 1
   VolumeOperator = "DiffusionLinearFEOperator"
   BoundaryOperator="DirichletMatrixCorrection"
}

MechanicsLinearFEOperator{
      name = "MechanicsLinearFEOperator"
      print_info_level = 1
      reset_reuses_matrix = TRUE
      InputVariable = "disp"
      OutputVariable = "disp"

      MechanicsElement {
       name="MechanicsLinearElement"
       USE_REDUCED_INTEGRATION = FALSE 
       FE_ORDER = "FIRST"  
       FE_FAMILY = "LAGRANGE"
       QRULE_TYPE = "QGAUSS"
       QRULE_ORDER = "DEFAULT"
      }

      LocalModel="MechanicsMaterialModel"
}


DiffusionLinearFEOperator{
    name = "DiffusionLinearFEOperator"
    InputVariable = "temperature"
    OutputVariable = "woohoo"
    print_info_level = 1
    reset_reuses_matrix = TRUE
    FixedConcentration = TRUE
    FixedBurnup = TRUE

    DiffusionElement {
        name = "DiffusionLinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

    LocalModel="DiffusionTransportModel"
}

MechanicsDirichletMatrixCorrection{
      name="DirichletMatrixCorrection"
      print_info_level = 1
      skip_params = FALSE
      skip_rhs_correction = TRUE
      number_of_ids = 1
      id_0          = 2
      number_of_dofs_0 =  3
      dof_0_0          =  0
      dof_0_1          =  1
      dof_0_2          =  2
   }

MechanicsMaterialModel { 
       name="IsotropicElasticModel"
       Youngs_Modulus = 2.08e11
       Poissons_Ratio = 0.23
}


DirichletMatrixCorrection{
    name="DirichletMatrixCorrection"
    skip_params = FALSE
    skip_rhs_correction = FALSE
    skip_rhs_add_correction = FALSE
    print_info_level = 3
    number_of_ids = 1
    id_0             = 1
    number_of_dofs_0 =   1
    dof_0_0          =   0
    value_0_0        = 300.
  }


DiffusionTransportModel{
        name = "DiffusionTransportModel"
        Material = "Independent"
        Property = "ThermalConductivity"
        Default_Concentration = .15
        Default_Temperature   = 300.
        Default_Burnup   = 0.0
} 

