numberOfOperators=2
dofsPerNode = 3, 1

testOperator1 {
   name="LinearBVPOperator"
   print_info_level = 1

   VolumeOperator = "MechanicsLinearFEOperator"
   BoundaryOperator="DirichletMatrixCorrection"
}

testOperator2 {
   name="LinearBVPOperator"
   print_info_level = 1
   VolumeOperator = "ThermalDiffusionLinearFEOperator"
   BoundaryOperator  = "RobinMatrixCorrection"
}

MechanicsLinearFEOperator{
      name = "MechanicsLinearFEOperator"
      print_info_level = 1
      reset_reuses_matrix = TRUE
      InputVariable = "disp"
      OutputVariable = "disp"

      MechanicsElement {
       name="MechanicsLinearElement"
       USE_REDUCED_INTEGRATION = FALSE 
       FE_ORDER = "FIRST"  
       FE_FAMILY = "LAGRANGE"
       QRULE_TYPE = "QGAUSS"
       QRULE_ORDER = "DEFAULT"
      }

      LocalModel="MechanicsMaterialModel"
}

DirichletMatrixCorrection{
      name="DirichletMatrixCorrection"
      print_info_level = 1
      skip_params = FALSE
      skip_rhs_correction = TRUE
      number_of_ids = 1
      id_0          = 2
      number_of_dofs_0 =  3
      dof_0_0          =  0
      dof_0_1          =  1
      dof_0_2          =  2
   }

MechanicsMaterialModel { 
       name="IsotropicElasticModel"
       Youngs_Modulus = 2.08e11
       Poissons_Ratio = 0.23
}

ThermalDiffusionLinearFEOperator{
    name = "DiffusionLinearFEOperator"
    InputVariable = "concentration"
    OutputVariable = "concentration"
    print_info_level = 1
    reset_reuses_matrix = TRUE
    FixedConcentration = TRUE
    FixedBurnup = TRUE

    DiffusionElement {
        name = "DiffusionLinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

    LocalModel="ThermalDiffusionTransportModel"
}

RobinMatrixCorrection{
   name="RobinMatrixCorrection"
   skip_params = FALSE
   FE_ORDER = "FIRST"
   FE_FAMILY = "LAGRANGE"
   QRULE_TYPE = "QGAUSS"
   QRULE_ORDER = "DEFAULT"
   alpha          = 1.0
   beta           = 1.0
   gamma          = 1.0
   print_info_level = 3
   number_of_ids    = 1
   id_0             = 2
   number_of_dofs_0 =   1
   dof_0_0          =   0
   value_0_0        = 101.
  }


ThermalDiffusionTransportModel{
        name = "DiffusionTransportModel"
        Material = "Independent"
        Property = "ThermalConductivity"
        Default_Concentration = .15
        Default_Temperature   = 300.
        Default_Burnup   = 0.0
} 


