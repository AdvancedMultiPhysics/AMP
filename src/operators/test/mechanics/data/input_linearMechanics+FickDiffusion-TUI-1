numberOfOperators=2
dofsPerNode = 3, 1

DiffusionTransportModel{
        name = "DiffusionTransportModel"
        Material = "Independent"
        Property = "FickCoefficient"
        Default_Concentration = .15
        Default_Temperature   = 300.
        Default_Burnup   = 0.0
    } 

MechanicsMaterialModel { 
       name="IsotropicElasticModel"
       Youngs_Modulus = 2.08e11
       Poissons_Ratio = 0.23
      }

testOperator1 {
      name = "MechanicsLinearFEOperator"
      print_info_level = 1
      reset_reuses_matrix = TRUE
      InputVariable = "disp"
      OutputVariable = "disp"

      MechanicsElement {
       name="MechanicsLinearElement"
       USE_REDUCED_INTEGRATION = FALSE 
       FE_ORDER = "FIRST"  
       FE_FAMILY = "LAGRANGE"
       QRULE_TYPE = "QGAUSS"
       QRULE_ORDER = "DEFAULT"
      }
 
      LocalModel = MechanicsMaterialModel
}
  
testOperator2 {
    name = "DiffusionLinearFEOperator"
    InputVariable = "concentration"
    OutputVariable = "concentration"
    print_info_level = 1
    reset_reuses_matrix = TRUE
    FixedTemperature = TRUE
    FixedBurnup = TRUE

    DiffusionElement {
        name = "DiffusionLinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }
 
    LocalModel = DiffusionTransportModel

}

