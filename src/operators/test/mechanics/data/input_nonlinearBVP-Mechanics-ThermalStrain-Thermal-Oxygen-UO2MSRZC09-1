    name="DirichletMatrixCorrection"
    skip_rhs_correction = FALSE
    skip_rhs_add_correction = FALSE
    skip_params = FALSE
    print_info_level = 3
    number_of_ids = 1
    id_0             = 1
    number_of_dofs_0 =   1
    dof_0_0          =   0
    value_0_0        = 300.
}

