
dofsPerNode = 1

testOperator {
   name="LinearBVPOperator"
   print_info_level = 1

   VolumeOperator = "DiffusionLinearFEOperator"
   BoundaryOperator="RobinMatrixCorrection"
}

DiffusionLinearFEOperator{

    name = "DiffusionLinearFEOperator"
    InputVariable = "concentration"
    OutputVariable = "concentration"
    print_info_level = 1
    reset_reuses_matrix = TRUE
    FixedTemperature = TRUE
    FixedBurnup = TRUE

    DiffusionElement {
        name = "DiffusionLinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

    LocalModel = "DiffusionTransportModel" 

}

RobinMatrixCorrection{
   name="RobinMatrixCorrection"
   skip_params = FALSE
   FE_ORDER = "FIRST"
   FE_FAMILY = "LAGRANGE"
   QRULE_TYPE = "QGAUSS"
   QRULE_ORDER = "DEFAULT"
   fConductivity  = 1.0
   alpha          = 1.0
   beta           = 1.0
   gamma          = 1.0
   print_info_level = 3
   number_of_ids    = 1
   id_0             = 2
   number_of_dofs_0 =   1
   dof_0_0          =   0
   value_0_0        = 101.
  }

DiffusionTransportModel{
        name = "DiffusionTransportModel"
        Material = "Ox_MSRZC_09"
        Property = "FickCoefficient"
        Default_Concentration = .1
        Default_Temperature   = 300.
