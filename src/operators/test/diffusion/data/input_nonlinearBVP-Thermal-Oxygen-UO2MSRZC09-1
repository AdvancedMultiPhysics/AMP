	   temperature="temperature"
	   concentration="concentration"
	 }
	 PrincipalVariable = 1
                Freezetemperature = FALSE

         LocalModel = "OxygenDiffusionTransportModel"
	
	 DiffusionElement {
	      name = "DiffusionNonlinearElement"
	      FE_ORDER = "FIRST"  
	      FE_FAMILY = "LAGRANGE"
	      QRULE_TYPE = "QGAUSS"
	      QRULE_ORDER = "DEFAULT"
	   }
}

