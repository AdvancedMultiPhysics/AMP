// Database for the mesh
Mesh {
    MeshName = "mesh"
    MeshType = "AMP"
    Generator = "cube"
    dim = 3
    Size = 8, 8, 17
    Range = -5, 5, -5, 5, -10, 10
    surfaceIds = 8, 8, 8, 8, 8, 8
    static = TRUE
}
  
LinearDiffusionOp{
    name = "DiffusionLinearFEOperator"
    InputVariable = "concentration"
    OutputVariable = "concentration"
    print_info_level = 1
    reset_reuses_matrix = TRUE
    FixedTemperature = TRUE
    FixedBurnup = TRUE

    DiffusionElement {
        name = "DiffusionLinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

    LocalModel = "DiffusionTransportModel"
}

NonlinearDiffusionOp{
    name = "DiffusionNonlinearFEOperator" 

    print_info_level = 1    
    ActiveInputVariables {
         concentration = "concentration"
    }
    OutputVariable = "concentration"
    PrincipalVariable = "concentration"   
  
    DiffusionElement{
        name = "DiffusionNonlinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

    LocalModel = "DiffusionTransportModel"
}

DiffusionTransportModel{
        name = "DiffusionTransportTensorModel"
        Material = "Independent"
        Dimensions = 3, 3
        Property = "TensorFickCoefficient"
        Parameters = 1., 2., 3., 4., 5., 6., 7., 8., 9.
} 
