// Database for the mesh
Mesh {
MeshName = "cube_3d"
MeshType = "AMP"
dim = 3
Generator = "cube"
Size = 4, 4, 4
Range = -5.12, 5.12, -5.12, 5.12, -5.12, 5.12
static = TRUE
}

ThermalNonlinearBVPOperator {
   name="NonlinearBVPOperator"
   print_info_level = 1
   VolumeOperator="DiffusionNonlinearFEOperator"
   BoundaryOperator = "DirichletVectorCorrection"
}

ThermalLinearBVPOperator {
   name="LinearBVPOperator"
   print_info_level = 1
  VolumeOperator="DiffusionLinearFEOperator"
  BoundaryOperator="DirichletMatrixCorrection"

}

DiffusionNonlinearFEOperator{
	 name="DiffusionNonlinearFEOperator"
	 InputVariable="temperature"
	 OutputVariable="temperature"
	 print_info_level = 1
	
	 ActiveInputVariables { 
	   temperature="temperature"
	 }
	 PrincipalVariable = "temperature"

         LocalModel = "DiffusionTransportModel"

         DiffusionElement {
	      name = "DiffusionNonlinearElement"
	      FE_ORDER = "FIRST"  
	      FE_FAMILY = "LAGRANGE"
	      QRULE_TYPE = "QGAUSS"
	      QRULE_ORDER = "DEFAULT"
	   }
}
   

DiffusionLinearFEOperator{
      name="DiffusionLinearFEOperator"
      InputVariable="linear"
      OutputVariable="linear"
      print_info_level = 3

     DiffusionElement {
       name = "DiffusionLinearElement"
       print_info_level = 3
       FE_ORDER = "FIRST"  
       FE_FAMILY = "LAGRANGE"
       QRULE_TYPE = "QGAUSS"
       QRULE_ORDER = "DEFAULT"
     }

     LocalModel="DiffusionTransportModel"
}

DirichletVectorCorrection{
     name = "DirichletVectorCorrection"
     print_info_level = 3
     skip_params = FALSE
     number_of_ids = 1
     id_0         =   1 
     number_of_dofs_0 =   1
     dof_0_0          =   0
     value_0_0        = 300.
}

DirichletMatrixCorrection{
    name="DirichletMatrixCorrection"
    skip_rhs_correction = FALSE
    skip_rhs_add_correction = FALSE
    skip_params = FALSE
    print_info_level = 3
    number_of_ids = 1
    id_0             = 1
    number_of_dofs_0 =   1
    dof_0_0          =   0
    value_0_0        = 300.
}

DiffusionTransportModel {
	     name = "DiffusionTransportModel"
	     Material = "Independent"	     
	     Property = "ThermalConductivity"
	     print_info_level = 3 
	     Default_Concentration = .15
	     Default_Temperature   = 300.
	     Default_Burnup   = 0.0
}
	
