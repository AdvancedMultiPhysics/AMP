// Database for the mesh
Mesh{
    MeshName = "mesh"
    MeshType = "libMesh"
    FileName = "brick.e"
    dim = 3
    NumberOfElements = 4096
}
  
LinearDiffusionOp{
    name = "DiffusionLinearFEOperator"
    InputVariable = "concentration"
    OutputVariable = "concentration"
    print_info_level = 1
    reset_reuses_matrix = TRUE
    FixedTemperature = TRUE
    FixedBurnup = TRUE

    DiffusionElement {
        name = "DiffusionLinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

    LocalModel = "DiffusionTransportModel"
}

NonlinearDiffusionOp{
    name = "DiffusionNonlinearFEOperator" 

    print_info_level = 1    
    ActiveInputVariables {
         concentration = "concentration"
    }
    OutputVariable = "concentration"
    PrincipalVariable = 1   
  
    DiffusionElement{
        name = "DiffusionNonlinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

    LocalModel = "DiffusionTransportModel"
}

DiffusionTransportModel{
        name = "DiffusionCylindricalTransportModel"
        Material = "CylindricallySymmetric"
        Property = "ScalarRadialFick"
 		RadiusArgument = "radius"
        Parameters = 1., 1., 1. 
} 
