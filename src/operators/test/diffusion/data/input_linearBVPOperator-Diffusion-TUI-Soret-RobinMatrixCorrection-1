
dofsPerNode = 1

testOperator {
   name="LinearBVPOperator"
   print_info_level = 1
 
   VolumeOperator = "DiffusionLinearFEOperator"
   BoundaryOperator="RobinMatrixCorrection"
}

DiffusionTransportModel{
    name = "DiffusionTransportModel"
    Material = "Independent"
    Property = "ThermalDiffusionCoefficient"
    Default_Concentration = .1
    Default_Temperature   = 300.
    Default_Burnup   = 0.0
} 

DiffusionLinearFEOperator{
    name = "DiffusionLinearFEOperator"
    InputVariable = "temperature"
    OutputVariable = "temperature"
    print_info_level = 1
    reset_reuses_matrix = TRUE
    FixedConcentration = TRUE
    FixedBurnup = TRUE

    DiffusionElement {
        name = "DiffusionLinearElement"
        TransportAtGaussPoints = TRUE
        FE_ORDER = "FIRST"  
        FE_FAMILY = "LAGRANGE"
        QRULE_TYPE = "QGAUSS"
        QRULE_ORDER = "DEFAULT"
    }    

    LocalModel=DiffusionTransportModel
}


RobinMatrixCorrection{
   name="RobinMatrixCorrection"
   skip_params = FALSE
   FE_ORDER = "FIRST"
   FE_FAMILY = "LAGRANGE"
   QRULE_TYPE = "QGAUSS"
   QRULE_ORDER = "DEFAULT"
   alpha          = 1.0
   beta           = 1.0
   gamma          = 1.0
   print_info_level = 2
   number_of_ids    = 1
   id_0             = 2
   number_of_dofs_0 =   1
   dof_0_0          =   0
   value_0_0        = 101.
  }

